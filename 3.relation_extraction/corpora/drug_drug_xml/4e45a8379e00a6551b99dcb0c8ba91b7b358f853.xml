  <chunk id="4e45a8379e00a6551b99dcb0c8ba91b7b358f853.c0" text="3D QSAR Pharmacophore Modeling, in Silico Screening, and Density Functional Theory (DFT) Approaches for Identification of Human Chymase Inhibitors">
    <entity charOffset="128-146" id="4e45a8379e00a6551b99dcb0c8ba91b7b358f853.c0.e0" ontology_id="CHEBI_64957" text="Chymase Inhibitors" type="chemical"/>
