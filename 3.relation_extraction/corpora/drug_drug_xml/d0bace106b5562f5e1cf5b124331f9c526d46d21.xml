  <chunk id="d0bace106b5562f5e1cf5b124331f9c526d46d21.c0" text="Rapid Evolution of PARP Genes Suggests a Broad Role for ADP-Ribosylation in Host-Virus Conflicts">
    <entity charOffset="19-23" id="d0bace106b5562f5e1cf5b124331f9c526d46d21.c0.e0" ontology_id="CHEBI_62913" text="PARP" type="chemical"/>
