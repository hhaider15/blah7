  <chunk id="c2b8b47dcfe70db8080da73d7a3941c94b31b025.c0" text="Evaluation of ViroCyt ® Virus Counter for Rapid Filovirus Quantitation">
    <entity charOffset="30-37" id="c2b8b47dcfe70db8080da73d7a3941c94b31b025.c0.e0" ontology_id="CHEBI_38960" text="Counter" type="chemical"/>
