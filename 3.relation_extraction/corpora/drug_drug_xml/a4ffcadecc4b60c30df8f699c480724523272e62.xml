  <chunk id="a4ffcadecc4b60c30df8f699c480724523272e62.c0" text="First Discovery and Stucture-Activity Relationship Study of Phenanthroquinolizidines as Novel Antiviral Agents against Tobacco Mosaic Virus (TMV)">
    <entity charOffset="94-103" id="a4ffcadecc4b60c30df8f699c480724523272e62.c0.e0" ontology_id="CHEBI_22587" text="Antiviral" type="chemical"/>
