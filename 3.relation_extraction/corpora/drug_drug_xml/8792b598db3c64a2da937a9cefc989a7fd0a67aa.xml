  <chunk id="8792b598db3c64a2da937a9cefc989a7fd0a67aa.c0" text="Multifaceted Regulation of Translational Readthrough by RNA Replication Elements in a Tombusvirus">
    <entity charOffset="56-59" id="8792b598db3c64a2da937a9cefc989a7fd0a67aa.c0.e0" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
    <entity charOffset="72-80" id="8792b598db3c64a2da937a9cefc989a7fd0a67aa.c0.e1" ontology_id="CHEBI_33250" text="Elements" type="chemical"/>
    <pair e1="8792b598db3c64a2da937a9cefc989a7fd0a67aa.c0.e0" e2="8792b598db3c64a2da937a9cefc989a7fd0a67aa.c0.e1" id="8792b598db3c64a2da937a9cefc989a7fd0a67aa.c0.p0" relation="true"/>
