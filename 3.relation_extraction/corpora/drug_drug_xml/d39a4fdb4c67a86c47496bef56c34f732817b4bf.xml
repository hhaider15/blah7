  <chunk id="d39a4fdb4c67a86c47496bef56c34f732817b4bf.c0" text="NSs Protein of Sandfly Fever Sicilian Phlebovirus Counteracts Interferon (IFN) Induction by Masking the DNA-Binding Domain of IFN Regulatory Factor 3 VIRUS-CELL INTERACTIONS crossm">
    <entity charOffset="4-11" id="d39a4fdb4c67a86c47496bef56c34f732817b4bf.c0.e0" ontology_id="CHEBI_16541" text="Protein" type="chemical"/>
