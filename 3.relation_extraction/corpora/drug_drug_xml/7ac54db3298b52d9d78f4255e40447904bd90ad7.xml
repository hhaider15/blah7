  <chunk id="7ac54db3298b52d9d78f4255e40447904bd90ad7.c0" text="Enterovirus 71 Induces Mitochondrial Reactive Oxygen Species Generation That is Required for Efficient Replication">
    <entity charOffset="37-60" id="7ac54db3298b52d9d78f4255e40447904bd90ad7.c0.e0" ontology_id="CHEBI_26523" text="Reactive Oxygen Species" type="chemical"/>
