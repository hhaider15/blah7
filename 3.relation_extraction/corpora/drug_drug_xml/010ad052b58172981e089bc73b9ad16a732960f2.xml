  <chunk id="010ad052b58172981e089bc73b9ad16a732960f2.c0" text="Activation of the Unfolded Protein Response Is Required for Defenses against Bacterial Pore-Forming Toxin In Vivo">
    <entity charOffset="27-34" id="010ad052b58172981e089bc73b9ad16a732960f2.c0.e0" ontology_id="CHEBI_16541" text="Protein" type="chemical"/>
    <entity charOffset="100-105" id="010ad052b58172981e089bc73b9ad16a732960f2.c0.e1" ontology_id="CHEBI_27026" text="Toxin" type="chemical"/>
    <pair e1="010ad052b58172981e089bc73b9ad16a732960f2.c0.e0" e2="010ad052b58172981e089bc73b9ad16a732960f2.c0.e1" id="010ad052b58172981e089bc73b9ad16a732960f2.c0.p0" relation="true"/>
