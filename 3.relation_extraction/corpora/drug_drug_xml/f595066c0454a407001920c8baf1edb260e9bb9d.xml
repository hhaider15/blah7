  <chunk id="f595066c0454a407001920c8baf1edb260e9bb9d.c0" text="RPiRLS: Quantitative Predictions of RNA Interacting with Any Protein of Known Sequence">
    <entity charOffset="36-39" id="f595066c0454a407001920c8baf1edb260e9bb9d.c0.e0" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
