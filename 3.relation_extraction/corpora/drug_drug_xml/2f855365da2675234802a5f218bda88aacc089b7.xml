  <chunk id="2f855365da2675234802a5f218bda88aacc089b7.c0" text="Lactococci and lactobacilli as mucosal delivery vectors for therapeutic proteins and DNA vaccines">
    <entity charOffset="72-80" id="2f855365da2675234802a5f218bda88aacc089b7.c0.e0" ontology_id="CHEBI_36080" text="proteins" type="chemical"/>
    <entity charOffset="85-88" id="2f855365da2675234802a5f218bda88aacc089b7.c0.e1" ontology_id="CHEBI_16991" text="DNA" type="chemical"/>
    <pair e1="2f855365da2675234802a5f218bda88aacc089b7.c0.e0" e2="2f855365da2675234802a5f218bda88aacc089b7.c0.e1" id="2f855365da2675234802a5f218bda88aacc089b7.c0.p0" relation="true"/>
