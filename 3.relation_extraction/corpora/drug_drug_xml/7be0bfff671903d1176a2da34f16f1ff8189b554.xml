  <chunk id="7be0bfff671903d1176a2da34f16f1ff8189b554.c0" text="The methyltransferase domain of dengue virus protein NS5 ensures efficient RNA synthesis initiation and elongation by the polymerase domain">
    <entity charOffset="45-52" id="7be0bfff671903d1176a2da34f16f1ff8189b554.c0.e0" ontology_id="CHEBI_16541" text="protein" type="chemical"/>
