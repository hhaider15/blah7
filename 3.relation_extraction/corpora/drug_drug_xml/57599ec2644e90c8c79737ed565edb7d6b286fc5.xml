  <chunk id="57599ec2644e90c8c79737ed565edb7d6b286fc5.c0" text="Nucleobase but not Sugar Fidelity is Maintained in the Sabin I RNA-Dependent RNA Polymerase">
    <entity charOffset="0-10" id="57599ec2644e90c8c79737ed565edb7d6b286fc5.c0.e0" ontology_id="CHEBI_18282" text="Nucleobase" type="chemical"/>
