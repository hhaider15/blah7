  <chunk id="bb3716d66667c66f6f79b37c765388cd334f2cad.c0" text="Tylosema esculentum (Marama) Tuber and Bean Extracts Are Strong Antiviral Agents against Rotavirus Infection">
    <entity charOffset="64-73" id="bb3716d66667c66f6f79b37c765388cd334f2cad.c0.e0" ontology_id="CHEBI_22587" text="Antiviral" type="chemical"/>
    <entity charOffset="64-80" id="bb3716d66667c66f6f79b37c765388cd334f2cad.c0.e1" ontology_id="CHEBI_22587" text="Antiviral Agents" type="chemical"/>
