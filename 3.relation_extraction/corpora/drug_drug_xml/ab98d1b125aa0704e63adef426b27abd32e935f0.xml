  <chunk id="ab98d1b125aa0704e63adef426b27abd32e935f0.c0" text="Full Genome Virus Detection in Fecal Samples Using Sensitive Nucleic Acid Preparation, Deep Sequencing, and a Novel Iterative Sequence Classification Algorithm">
    <entity charOffset="61-73" id="ab98d1b125aa0704e63adef426b27abd32e935f0.c0.e0" ontology_id="CHEBI_33696" text="Nucleic Acid" type="chemical"/>
