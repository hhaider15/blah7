  <chunk id="c5c2bc7a07670d6fb970d84a59aab3832752a3f1.c0" text="Role of the ERK1/2 Signaling Pathway in the Replication of Junín and Tacaribe Viruses">
    <entity charOffset="0-4" id="c5c2bc7a07670d6fb970d84a59aab3832752a3f1.c0.e0" ontology_id="CHEBI_50906" text="Role" type="chemical"/>
