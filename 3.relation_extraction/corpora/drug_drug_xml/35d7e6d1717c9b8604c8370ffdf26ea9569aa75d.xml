  <chunk id="35d7e6d1717c9b8604c8370ffdf26ea9569aa75d.c0" text="Influenza A Virus Encoding Secreted Gaussia Luciferase as Useful Tool to Analyze Viral Replication and Its Inhibition by Antiviral Compounds and Cellular Proteins">
    <entity charOffset="121-130" id="35d7e6d1717c9b8604c8370ffdf26ea9569aa75d.c0.e0" ontology_id="CHEBI_22587" text="Antiviral" type="chemical"/>
    <entity charOffset="154-162" id="35d7e6d1717c9b8604c8370ffdf26ea9569aa75d.c0.e1" ontology_id="CHEBI_36080" text="Proteins" type="chemical"/>
    <pair e1="35d7e6d1717c9b8604c8370ffdf26ea9569aa75d.c0.e0" e2="35d7e6d1717c9b8604c8370ffdf26ea9569aa75d.c0.e1" id="35d7e6d1717c9b8604c8370ffdf26ea9569aa75d.c0.p0" relation="true"/>
