  <chunk id="54cbbaf44335fdc1995cb19e5a08779929fd0966.c0" text="HIV-1 Fusion Is Blocked through Binding of GB Virus C E2D Peptides to the HIV-1 gp41 Disulfide Loop">
    <entity charOffset="58-66" id="54cbbaf44335fdc1995cb19e5a08779929fd0966.c0.e0" ontology_id="CHEBI_16670" text="Peptides" type="chemical"/>
    <entity charOffset="85-94" id="54cbbaf44335fdc1995cb19e5a08779929fd0966.c0.e1" ontology_id="CHEBI_48343" text="Disulfide" type="chemical"/>
