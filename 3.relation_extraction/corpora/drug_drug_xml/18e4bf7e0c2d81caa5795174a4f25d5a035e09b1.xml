  <chunk id="18e4bf7e0c2d81caa5795174a4f25d5a035e09b1.c0" text="A Novel Peptide Enhances Therapeutic Efficacy of Liposomal Anti-Cancer Drugs in Mice Models of Human Lung Cancer">
    <entity charOffset="8-15" id="18e4bf7e0c2d81caa5795174a4f25d5a035e09b1.c0.e0" ontology_id="CHEBI_16670" text="Peptide" type="chemical"/>
