    <entity charOffset="114-124" id="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c0.e2" ontology_id="CHEBI_35222" text="Inhibitors" type="chemical"/>
    <pair e1="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c0.e0" e2="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c0.e1" id="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c0.p0" relation="true"/>
    <pair e1="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c0.e0" e2="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c0.e2" id="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c0.p1" relation="true"/>
