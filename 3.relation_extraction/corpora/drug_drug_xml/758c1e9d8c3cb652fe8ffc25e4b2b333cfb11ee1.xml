    <pair e1="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c3.e1" e2="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c3.e2" id="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c3.p2" relation="true"/>
    <pair e1="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c3.e1" e2="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c3.e3" id="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c3.p3" relation="true"/>
  </chunk>
  <chunk id="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c4" text="Our data suggest that the synthetic peptide FP5 could serve as a valuable addition to the current FIP prevention methods.">
    <entity charOffset="36-43" id="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c4.e0" ontology_id="CHEBI_16670" text="peptide" type="chemical"/>
