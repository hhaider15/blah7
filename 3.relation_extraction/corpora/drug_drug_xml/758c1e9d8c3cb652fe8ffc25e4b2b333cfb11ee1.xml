  <chunk id="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c0" text="Peptides Corresponding to the Predicted Heptad Repeat 2 Domain of the Feline Coronavirus Spike Protein Are Potent Inhibitors of Viral Infection">
    <entity charOffset="0-8" id="758c1e9d8c3cb652fe8ffc25e4b2b333cfb11ee1.c0.e0" ontology_id="CHEBI_16670" text="Peptides" type="chemical"/>
