  <chunk id="b6682adfe5026c78e3d59b165126abfa8945eb5f.c0" text="Dendritic Cell Immunoreceptor Is a New Target for Anti- AIDS Drug Development: Identification of DCIR/HIV-1 Inhibitors">
    <entity charOffset="61-65" id="b6682adfe5026c78e3d59b165126abfa8945eb5f.c0.e0" ontology_id="CHEBI_23888" text="Drug" type="chemical"/>
    <entity charOffset="108-118" id="b6682adfe5026c78e3d59b165126abfa8945eb5f.c0.e1" ontology_id="CHEBI_35222" text="Inhibitors" type="chemical"/>
    <pair e1="b6682adfe5026c78e3d59b165126abfa8945eb5f.c0.e0" e2="b6682adfe5026c78e3d59b165126abfa8945eb5f.c0.e1" id="b6682adfe5026c78e3d59b165126abfa8945eb5f.c0.p0" relation="true"/>
