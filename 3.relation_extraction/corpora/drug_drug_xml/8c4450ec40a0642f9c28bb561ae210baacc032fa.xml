  <chunk id="8c4450ec40a0642f9c28bb561ae210baacc032fa.c0" text="Middle east respiratory syndrome corona virus spike glycoprotein suppresses macrophage responses via DPP4- mediated induction of IRAK-M and PPARγ">
    <entity charOffset="33-39" id="8c4450ec40a0642f9c28bb561ae210baacc032fa.c0.e0" ontology_id="CHEBI_37409" text="corona" type="chemical"/>
    <entity charOffset="52-64" id="8c4450ec40a0642f9c28bb561ae210baacc032fa.c0.e1" ontology_id="CHEBI_17089" text="glycoprotein" type="chemical"/>
    <pair e1="8c4450ec40a0642f9c28bb561ae210baacc032fa.c0.e0" e2="8c4450ec40a0642f9c28bb561ae210baacc032fa.c0.e1" id="8c4450ec40a0642f9c28bb561ae210baacc032fa.c0.p0" relation="true"/>
