  <chunk id="6d20ab75d7394461e9acd16899bd55902177d8fe.c0" text="Initiation of human astrovirus type 1 infection was blocked by inhibitors of phosphoinositide 3-kinase">
    <entity charOffset="63-73" id="6d20ab75d7394461e9acd16899bd55902177d8fe.c0.e0" ontology_id="CHEBI_35222" text="inhibitors" type="chemical"/>
    <entity charOffset="77-93" id="6d20ab75d7394461e9acd16899bd55902177d8fe.c0.e1" ontology_id="CHEBI_16749" text="phosphoinositide" type="chemical"/>
