  <chunk id="514e24bfc28e0b5cf4b111d94604e7aa983500c9.c0" text="MERS-CoV 4b protein interferes with the NF- κB-dependent innate immune response during infection Author summary">
    <entity charOffset="12-19" id="514e24bfc28e0b5cf4b111d94604e7aa983500c9.c0.e0" ontology_id="CHEBI_16541" text="protein" type="chemical"/>
