  <chunk id="90570e69d4ac4b2e72385123ba09f6b754707b61.c0" text="Extra Dose of Vitamin C Based on a Daily Supplementation Shortens the Common Cold: A Meta-Analysis of 9 Randomized Controlled Trials">
    <entity charOffset="14-21" id="90570e69d4ac4b2e72385123ba09f6b754707b61.c0.e0" ontology_id="CHEBI_33229" text="Vitamin" type="chemical"/>
