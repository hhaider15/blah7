  <chunk id="9d941db9f06f446ae113c5f76f7597553a2f365b.c0" text="Overexpression of the nucleocapsid protein of Middle East respiratory syndrome coronavirus up-regulates CXCL10">
    <entity charOffset="35-42" id="9d941db9f06f446ae113c5f76f7597553a2f365b.c0.e0" ontology_id="CHEBI_16541" text="protein" type="chemical"/>
