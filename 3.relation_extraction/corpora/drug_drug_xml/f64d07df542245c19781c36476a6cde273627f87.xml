    <entity charOffset="100-108" id="f64d07df542245c19781c36476a6cde273627f87.c0.e1" ontology_id="CHEBI_37943" text="Colistin" type="chemical"/>
    <pair e1="f64d07df542245c19781c36476a6cde273627f87.c0.e0" e2="f64d07df542245c19781c36476a6cde273627f87.c0.e1" id="f64d07df542245c19781c36476a6cde273627f87.c0.p0" relation="true"/>
