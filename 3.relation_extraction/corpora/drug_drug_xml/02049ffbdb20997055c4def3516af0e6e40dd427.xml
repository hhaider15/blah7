  <chunk id="02049ffbdb20997055c4def3516af0e6e40dd427.c0" text="Ribonuclease L mediates the cell-lethal phenotype of double-stranded RNA editing enzyme ADAR1 deficiency in a human cell line">
    <entity charOffset="53-72" id="02049ffbdb20997055c4def3516af0e6e40dd427.c0.e0" ontology_id="CHEBI_67208" text="double-stranded RNA" type="chemical"/>
