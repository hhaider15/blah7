  <chunk id="d3f515c47fec377ecc7f3961948916239e4e079f.c0" text="Citation: Epstein-Barr Virus Nuclear Antigen 1 Recruits Cyclophilin A to Facilitate the Replication of Viral DNA Genome">
    <entity charOffset="37-44" id="d3f515c47fec377ecc7f3961948916239e4e079f.c0.e0" ontology_id="CHEBI_59132" text="Antigen" type="chemical"/>
