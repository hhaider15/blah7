  <chunk id="bd92cbae7179f07d59d1ce4d7ca96e37ebb40ec9.c0" text="Design of Wide-Spectrum Inhibitors Targeting Coronavirus Main Proteases">
    <entity charOffset="24-34" id="bd92cbae7179f07d59d1ce4d7ca96e37ebb40ec9.c0.e0" ontology_id="CHEBI_35222" text="Inhibitors" type="chemical"/>
