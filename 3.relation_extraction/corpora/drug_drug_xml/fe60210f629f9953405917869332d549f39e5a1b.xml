  <chunk id="fe60210f629f9953405917869332d549f39e5a1b.c0" text="The Structure of a Rigorously Conserved RNA Element within the SARS Virus Genome">
    <entity charOffset="40-43" id="fe60210f629f9953405917869332d549f39e5a1b.c0.e0" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
