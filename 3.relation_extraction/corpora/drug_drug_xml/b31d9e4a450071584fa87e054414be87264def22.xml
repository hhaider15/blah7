  <chunk id="b31d9e4a450071584fa87e054414be87264def22.c0" text="Vitamin D 3 and gargling for the prevention of upper respiratory tract infections: a randomized controlled trial">
    <entity charOffset="0-7" id="b31d9e4a450071584fa87e054414be87264def22.c0.e0" ontology_id="CHEBI_33229" text="Vitamin" type="chemical"/>
