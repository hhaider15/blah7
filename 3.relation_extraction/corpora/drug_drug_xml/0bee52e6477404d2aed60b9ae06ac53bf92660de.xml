  <chunk id="0bee52e6477404d2aed60b9ae06ac53bf92660de.c0" text="The Hemagglutinin-Esterase Fusion Glycoprotein Is a Primary Determinant of the Exceptional Thermal and Acid Stability of Influenza D Virus">
    <entity charOffset="34-46" id="0bee52e6477404d2aed60b9ae06ac53bf92660de.c0.e0" ontology_id="CHEBI_17089" text="Glycoprotein" type="chemical"/>
