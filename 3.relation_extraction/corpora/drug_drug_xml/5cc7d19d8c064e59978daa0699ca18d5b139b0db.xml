  <chunk id="5cc7d19d8c064e59978daa0699ca18d5b139b0db.c0" text="Human β-defensin 2 plays a regulatory role in innate antiviral immunity and is capable of potentiating the induction of antigen- specific immunity">
    <entity charOffset="38-42" id="5cc7d19d8c064e59978daa0699ca18d5b139b0db.c0.e0" ontology_id="CHEBI_50906" text="role" type="chemical"/>
    <entity charOffset="53-62" id="5cc7d19d8c064e59978daa0699ca18d5b139b0db.c0.e1" ontology_id="CHEBI_22587" text="antiviral" type="chemical"/>
