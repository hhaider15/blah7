  <chunk id="9fd83230962e678e8ec4d5dffee0bb431e19c2d9.c0" text="Metagenomic analysis of viral nucleic acid extraction methods in respiratory clinical samples">
    <entity charOffset="30-42" id="9fd83230962e678e8ec4d5dffee0bb431e19c2d9.c0.e0" ontology_id="CHEBI_33696" text="nucleic acid" type="chemical"/>
