  <chunk id="75e5e406f83b2f0a94abc8cbed71cbb900f9956e.c0" text="The Role of Electron Microscopy in Studying the Continuum of Changes in Membranous Structures during Poliovirus Infection">
    <entity charOffset="4-8" id="75e5e406f83b2f0a94abc8cbed71cbb900f9956e.c0.e0" ontology_id="CHEBI_50906" text="Role" type="chemical"/>
    <entity charOffset="12-20" id="75e5e406f83b2f0a94abc8cbed71cbb900f9956e.c0.e1" ontology_id="CHEBI_10545" text="Electron" type="chemical"/>
    <pair e1="75e5e406f83b2f0a94abc8cbed71cbb900f9956e.c0.e0" e2="75e5e406f83b2f0a94abc8cbed71cbb900f9956e.c0.e1" id="75e5e406f83b2f0a94abc8cbed71cbb900f9956e.c0.p0" relation="true"/>
