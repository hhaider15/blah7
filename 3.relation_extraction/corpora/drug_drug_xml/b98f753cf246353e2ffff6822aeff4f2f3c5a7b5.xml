  <chunk id="b98f753cf246353e2ffff6822aeff4f2f3c5a7b5.c0" text="Antiviral Phosphorodiamidate Morpholino Oligomers are Protective against Chikungunya Virus Infection on Cell-based and Murine Models">
    <entity charOffset="0-9" id="b98f753cf246353e2ffff6822aeff4f2f3c5a7b5.c0.e0" ontology_id="CHEBI_22587" text="Antiviral" type="chemical"/>
    <entity charOffset="40-49" id="b98f753cf246353e2ffff6822aeff4f2f3c5a7b5.c0.e1" ontology_id="CHEBI_132554" text="Oligomers" type="chemical"/>
    <pair e1="b98f753cf246353e2ffff6822aeff4f2f3c5a7b5.c0.e0" e2="b98f753cf246353e2ffff6822aeff4f2f3c5a7b5.c0.e1" id="b98f753cf246353e2ffff6822aeff4f2f3c5a7b5.c0.p0" relation="true"/>
