  <chunk id="1ec08de8b92b3b6c9be98dcd92db0ddf3efdad44.c0" text="Immunomodulator expression in trophoblasts from the feline immunodeficiency virus (FIV)- infected cat">
    <entity charOffset="0-15" id="1ec08de8b92b3b6c9be98dcd92db0ddf3efdad44.c0.e0" ontology_id="CHEBI_50846" text="Immunomodulator" type="chemical"/>
