  <chunk id="023dff96542ca1e77e3a1d08a2f3893ca1ebc057.c0" text="Sequences, Annotation and Single Nucleotide Polymorphism of the Major Histocompatibility Complex in the Domestic Cat">
    <entity charOffset="33-43" id="023dff96542ca1e77e3a1d08a2f3893ca1ebc057.c0.e0" ontology_id="CHEBI_36976" text="Nucleotide" type="chemical"/>
