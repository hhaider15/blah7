  <chunk id="fd5e3020671499cde830bd25de618009ece0af13.c0" text="Antiviral and Neuroprotective Role of Octaguanidinium Dendrimer-Conjugated Morpholino Oligomers in Japanese Encephalitis">
    <entity charOffset="0-9" id="fd5e3020671499cde830bd25de618009ece0af13.c0.e0" ontology_id="CHEBI_22587" text="Antiviral" type="chemical"/>
