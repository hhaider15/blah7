  <chunk id="8deca5b29c3d3c0b11f1c9032a981f95dc8d1619.c0" text="Rapid Identification of Novel Immunodominant Proteins and Characterization of a Specific Linear Epitope of Campylobacter jejuni">
    <entity charOffset="45-53" id="8deca5b29c3d3c0b11f1c9032a981f95dc8d1619.c0.e0" ontology_id="CHEBI_36080" text="Proteins" type="chemical"/>
    <entity charOffset="96-103" id="8deca5b29c3d3c0b11f1c9032a981f95dc8d1619.c0.e1" ontology_id="CHEBI_53000" text="Epitope" type="chemical"/>
    <pair e1="8deca5b29c3d3c0b11f1c9032a981f95dc8d1619.c0.e0" e2="8deca5b29c3d3c0b11f1c9032a981f95dc8d1619.c0.e1" id="8deca5b29c3d3c0b11f1c9032a981f95dc8d1619.c0.p0" relation="true"/>
