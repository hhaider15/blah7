  <chunk id="c0cf1c6377c3115aa80ff787332120d4ba1cd386.c0" text="Coronaviruses as DNA Wannabes: A New Model for the Regulation of RNA Virus Replication Fidelity">
    <entity charOffset="17-20" id="c0cf1c6377c3115aa80ff787332120d4ba1cd386.c0.e0" ontology_id="CHEBI_16991" text="DNA" type="chemical"/>
