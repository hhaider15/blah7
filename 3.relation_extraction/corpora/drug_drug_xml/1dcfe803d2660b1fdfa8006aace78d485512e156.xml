  <chunk id="1dcfe803d2660b1fdfa8006aace78d485512e156.c0" text="Structural and Functional Insights into Small, Glutamine-Rich, Tetratricopeptide Repeat Protein Alpha">
    <entity charOffset="88-95" id="1dcfe803d2660b1fdfa8006aace78d485512e156.c0.e0" ontology_id="CHEBI_16541" text="Protein" type="chemical"/>
    <entity charOffset="96-101" id="1dcfe803d2660b1fdfa8006aace78d485512e156.c0.e1" ontology_id="CHEBI_30216" text="Alpha" type="chemical"/>
    <pair e1="1dcfe803d2660b1fdfa8006aace78d485512e156.c0.e0" e2="1dcfe803d2660b1fdfa8006aace78d485512e156.c0.e1" id="1dcfe803d2660b1fdfa8006aace78d485512e156.c0.p0" relation="true"/>
