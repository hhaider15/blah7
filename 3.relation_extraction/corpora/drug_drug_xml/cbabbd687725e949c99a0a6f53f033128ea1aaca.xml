  <chunk id="cbabbd687725e949c99a0a6f53f033128ea1aaca.c0" text="Precisely Molded Nanoparticle Displaying DENV-E Proteins Induces Robust Serotype- Specific Neutralizing Antibody Responses">
    <entity charOffset="17-29" id="cbabbd687725e949c99a0a6f53f033128ea1aaca.c0.e0" ontology_id="CHEBI_50803" text="Nanoparticle" type="chemical"/>
