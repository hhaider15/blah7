  <chunk id="1297127819be12e7f5319edd0a9b35fd5cc566b7.c0" text="An RNA Aptamer Provides a Novel Approach for the Induction of Apoptosis by Targeting the HPV16 E7 Oncoprotein">
    <entity charOffset="3-6" id="1297127819be12e7f5319edd0a9b35fd5cc566b7.c0.e0" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
