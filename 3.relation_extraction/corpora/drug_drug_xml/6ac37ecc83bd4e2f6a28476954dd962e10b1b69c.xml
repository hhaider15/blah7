  <chunk id="6ac37ecc83bd4e2f6a28476954dd962e10b1b69c.c0" text="ARTICLE Molecularly specific detection of bacterial lipoteichoic acid for diagnosis of prosthetic joint infection of the bone">
    <entity charOffset="52-69" id="6ac37ecc83bd4e2f6a28476954dd962e10b1b69c.c0.e0" ontology_id="CHEBI_28640" text="lipoteichoic acid" type="chemical"/>
