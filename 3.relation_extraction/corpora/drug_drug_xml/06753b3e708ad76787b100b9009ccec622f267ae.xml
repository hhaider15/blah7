  <chunk id="06753b3e708ad76787b100b9009ccec622f267ae.c0" text="Emodin Inhibits EBV Reactivation and Represses NPC Tumorigenesis">
    <entity charOffset="0-6" id="06753b3e708ad76787b100b9009ccec622f267ae.c0.e0" ontology_id="CHEBI_42223" text="Emodin" type="chemical"/>
    <entity charOffset="47-50" id="06753b3e708ad76787b100b9009ccec622f267ae.c0.e1" ontology_id="CHEBI_80552" text="NPC" type="chemical"/>
    <pair e1="06753b3e708ad76787b100b9009ccec622f267ae.c0.e0" e2="06753b3e708ad76787b100b9009ccec622f267ae.c0.e1" id="06753b3e708ad76787b100b9009ccec622f267ae.c0.p0" relation="true"/>
