  <chunk id="9e881f35d3222748d7c2cb9a2a8b9b0d99246564.c0" text="Immortalization and Characterization of Porcine Macrophages that had Been transduced with Lentiviral Vectors encoding the sV40 Large t Antigen and Porcine telomerase Reverse transcriptase">
    <entity charOffset="133-142" id="9e881f35d3222748d7c2cb9a2a8b9b0d99246564.c0.e0" ontology_id="CHEBI_16981" text="t Antigen" type="chemical"/>
