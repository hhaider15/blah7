  <chunk id="03ca47766e6f3e8d5d71fa25d86803f82d690b38.c0" text="Myricitrin Protects Cardiomyocytes from Hypoxia/Reoxygenation Injury: Involvement of Heat Shock Protein 90">
    <entity charOffset="0-10" id="03ca47766e6f3e8d5d71fa25d86803f82d690b38.c0.e0" ontology_id="CHEBI_70082" text="Myricitrin" type="chemical"/>
