  <chunk id="42c1d88111ece7fdf4a80dc8ddb30be44b211d80.c0" text="Rhesus Macaque Theta Defensins Suppress Inflammatory Cytokines and Enhance Survival in Mouse Models of Bacteremic Sepsis">
    <entity charOffset="15-20" id="42c1d88111ece7fdf4a80dc8ddb30be44b211d80.c0.e0" ontology_id="CHEBI_138143" text="Theta" type="chemical"/>
    <entity charOffset="21-30" id="42c1d88111ece7fdf4a80dc8ddb30be44b211d80.c0.e1" ontology_id="CHEBI_82761" text="Defensins" type="chemical"/>
    <pair e1="42c1d88111ece7fdf4a80dc8ddb30be44b211d80.c0.e0" e2="42c1d88111ece7fdf4a80dc8ddb30be44b211d80.c0.e1" id="42c1d88111ece7fdf4a80dc8ddb30be44b211d80.c0.p0" relation="true"/>
