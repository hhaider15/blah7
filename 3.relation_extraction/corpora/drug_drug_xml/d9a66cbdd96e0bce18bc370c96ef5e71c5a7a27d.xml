  <chunk id="d9a66cbdd96e0bce18bc370c96ef5e71c5a7a27d.c0" text="molecules Efficient Separation of Four Antibacterial Diterpenes from the Roots of Salvia Prattii Using Non-Aqueous Hydrophilic Solid-Phase Extraction Followed by Preparative High-Performance Liquid Chromatography">
    <entity charOffset="0-9" id="d9a66cbdd96e0bce18bc370c96ef5e71c5a7a27d.c0.e0" ontology_id="CHEBI_25367" text="molecules" type="chemical"/>
