  <chunk id="f601b810174ec3f6056d0806959c06a2b211f3ca.c0" text="Corticosteroid suppression of antiviral immunity increases bacterial loads and mucus production in COPD exacerbations">
    <entity charOffset="0-14" id="f601b810174ec3f6056d0806959c06a2b211f3ca.c0.e0" ontology_id="CHEBI_50858" text="Corticosteroid" type="chemical"/>
