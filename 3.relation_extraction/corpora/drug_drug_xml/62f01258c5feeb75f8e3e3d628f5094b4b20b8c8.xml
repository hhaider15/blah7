  <chunk id="62f01258c5feeb75f8e3e3d628f5094b4b20b8c8.c0" text="molecules In Vitro and in Vivo Studies of the Inhibitory Effects of Emodin Isolated from Polygonum cuspidatum on Coxsakievirus B 4">
    <entity charOffset="0-9" id="62f01258c5feeb75f8e3e3d628f5094b4b20b8c8.c0.e0" ontology_id="CHEBI_25367" text="molecules" type="chemical"/>
