  <chunk id="9c438a13c9083e842b860ae7b477e671e60fb4cb.c0" text="Clinical Medicine Novel Pharmacological Activity of Artesunate and Artemisinin: Their Potential as Anti-Tubercular Agents">
    <entity charOffset="9-17" id="9c438a13c9083e842b860ae7b477e671e60fb4cb.c0.e0" ontology_id="CHEBI_23888" text="Medicine" type="chemical"/>
