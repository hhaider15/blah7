  <chunk id="3b1fca9311e0efc4368f3775dd878f57c5978a5e.c0" text="The Effects of Simvastatin or Interferon-a on Infectivity of Human Norovirus Using a Gnotobiotic Pig Model for the Study of Antivirals">
    <entity charOffset="15-26" id="3b1fca9311e0efc4368f3775dd878f57c5978a5e.c0.e0" ontology_id="CHEBI_9150" text="Simvastatin" type="chemical"/>
