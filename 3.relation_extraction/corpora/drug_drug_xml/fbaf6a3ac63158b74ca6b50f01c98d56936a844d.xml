  <chunk id="fbaf6a3ac63158b74ca6b50f01c98d56936a844d.c0" text="RNA decay during gammaherpesvirus infection reduces RNA polymerase II occupancy of host promoters but spares viral promoters">
    <entity charOffset="0-3" id="fbaf6a3ac63158b74ca6b50f01c98d56936a844d.c0.e0" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
    <entity charOffset="52-55" id="fbaf6a3ac63158b74ca6b50f01c98d56936a844d.c0.e1" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
