  <chunk id="04f4cff6c1a32e0edb50bdb8daf00f82708a5142.c0" text="Plasmodium vivax Antigen Discovery Based on Alpha-Helical Coiled Coil Protein Motif">
    <entity charOffset="17-24" id="04f4cff6c1a32e0edb50bdb8daf00f82708a5142.c0.e0" ontology_id="CHEBI_59132" text="Antigen" type="chemical"/>
