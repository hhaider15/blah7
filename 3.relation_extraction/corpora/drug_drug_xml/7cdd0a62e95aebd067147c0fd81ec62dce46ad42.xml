  <chunk id="7cdd0a62e95aebd067147c0fd81ec62dce46ad42.c0" text="The Natural Compound Homoharringtonine Presents Broad Antiviral Activity In Vitro and In Vivo">
    <entity charOffset="21-38" id="7cdd0a62e95aebd067147c0fd81ec62dce46ad42.c0.e0" ontology_id="CHEBI_71019" text="Homoharringtonine" type="chemical"/>
    <entity charOffset="54-63" id="7cdd0a62e95aebd067147c0fd81ec62dce46ad42.c0.e1" ontology_id="CHEBI_22587" text="Antiviral" type="chemical"/>
