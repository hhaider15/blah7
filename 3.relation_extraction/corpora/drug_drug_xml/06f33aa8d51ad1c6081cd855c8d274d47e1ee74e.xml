  <chunk id="06f33aa8d51ad1c6081cd855c8d274d47e1ee74e.c0" text="Cytoplasmic Translocation of Polypyrimidine Tract-Binding Protein and Its Binding to Viral RNA during Japanese Encephalitis Virus Infection Inhibits Virus Replication">
    <entity charOffset="58-65" id="06f33aa8d51ad1c6081cd855c8d274d47e1ee74e.c0.e0" ontology_id="CHEBI_16541" text="Protein" type="chemical"/>
