    <entity charOffset="19-39" id="07420d39191900a262a4c5afff61e0ef80eac575.c0.e1" ontology_id="CHEBI_133334" text="Small Nucleolar RNAs" type="chemical"/>
    <pair e1="07420d39191900a262a4c5afff61e0ef80eac575.c0.e0" e2="07420d39191900a262a4c5afff61e0ef80eac575.c0.e1" id="07420d39191900a262a4c5afff61e0ef80eac575.c0.p0" relation="true"/>
