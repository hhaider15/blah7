  <chunk id="a2e8d9291e399c6d8adc6d39318de197e86b3f4e.c0" text="Antiviral Activity of Glycyrrhizin against Hepatitis C Virus In Vitro">
    <entity charOffset="0-9" id="a2e8d9291e399c6d8adc6d39318de197e86b3f4e.c0.e0" ontology_id="CHEBI_22587" text="Antiviral" type="chemical"/>
    <entity charOffset="22-34" id="a2e8d9291e399c6d8adc6d39318de197e86b3f4e.c0.e1" ontology_id="CHEBI_15939" text="Glycyrrhizin" type="chemical"/>
    <pair e1="a2e8d9291e399c6d8adc6d39318de197e86b3f4e.c0.e0" e2="a2e8d9291e399c6d8adc6d39318de197e86b3f4e.c0.e1" id="a2e8d9291e399c6d8adc6d39318de197e86b3f4e.c0.p0" relation="true"/>
