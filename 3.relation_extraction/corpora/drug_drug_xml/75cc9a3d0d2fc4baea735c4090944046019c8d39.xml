  <chunk id="75cc9a3d0d2fc4baea735c4090944046019c8d39.c0" text="A Novel Enediynyl Peptide Inhibitor of Furin That Blocks Processing of proPDGF-A, B and proVEGF-C">
    <entity charOffset="18-25" id="75cc9a3d0d2fc4baea735c4090944046019c8d39.c0.e0" ontology_id="CHEBI_16670" text="Peptide" type="chemical"/>
