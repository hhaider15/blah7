  <chunk id="399cef5a5c8ca96a0ac33ee31dec722eac145a7b.c0" text="FLDS: A Comprehensive dsRNA Sequencing Method for Intracellular RNA Virus Surveillance">
    <entity charOffset="22-27" id="399cef5a5c8ca96a0ac33ee31dec722eac145a7b.c0.e0" ontology_id="CHEBI_67208" text="dsRNA" type="chemical"/>
