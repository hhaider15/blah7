  <chunk id="44f3796bf7bdaa361b977d350bc71190b31cf558.c0" text="Immunome Research Analysis and prediction of protective continuous B-cell epitopes on pathogen proteins">
    <entity charOffset="95-103" id="44f3796bf7bdaa361b977d350bc71190b31cf558.c0.e0" ontology_id="CHEBI_36080" text="proteins" type="chemical"/>
