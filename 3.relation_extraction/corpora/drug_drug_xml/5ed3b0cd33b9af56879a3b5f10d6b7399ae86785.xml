  <chunk id="5ed3b0cd33b9af56879a3b5f10d6b7399ae86785.c0" text="Nanopolymers improve delivery of exon skipping oligonucleotides and concomitant dystrophin expression in skeletal muscle of mdx mice">
    <entity charOffset="47-63" id="5ed3b0cd33b9af56879a3b5f10d6b7399ae86785.c0.e0" ontology_id="CHEBI_7754" text="oligonucleotides" type="chemical"/>
