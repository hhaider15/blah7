  <chunk id="836ec77a81730c540b750253cc4ee9f8fed0fb49.c0" text="Conformational B-Cell Epitope Prediction Method Based on Antigen Preprocessing and Mimotopes Analysis">
    <entity charOffset="22-29" id="836ec77a81730c540b750253cc4ee9f8fed0fb49.c0.e0" ontology_id="CHEBI_53000" text="Epitope" type="chemical"/>
