  <chunk id="10a769cac0a76b41a6a82be148c1a2675e9d4604.c0" text="TIM-family Proteins Promote Infection of Multiple Enveloped Viruses through Virion-associated Phosphatidylserine">
    <entity charOffset="11-19" id="10a769cac0a76b41a6a82be148c1a2675e9d4604.c0.e0" ontology_id="CHEBI_36080" text="Proteins" type="chemical"/>
    <entity charOffset="94-112" id="10a769cac0a76b41a6a82be148c1a2675e9d4604.c0.e1" ontology_id="CHEBI_18303" text="Phosphatidylserine" type="chemical"/>
    <pair e1="10a769cac0a76b41a6a82be148c1a2675e9d4604.c0.e0" e2="10a769cac0a76b41a6a82be148c1a2675e9d4604.c0.e1" id="10a769cac0a76b41a6a82be148c1a2675e9d4604.c0.p0" relation="true"/>
