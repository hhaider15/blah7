  <chunk id="ba502a88e3f83aa9e3d5edd649970db6da85c5bf.c0" text="The role of CXCL10 in the pathogenesis of experimental septic shock">
    <entity charOffset="4-8" id="ba502a88e3f83aa9e3d5edd649970db6da85c5bf.c0.e0" ontology_id="CHEBI_50906" text="role" type="chemical"/>
