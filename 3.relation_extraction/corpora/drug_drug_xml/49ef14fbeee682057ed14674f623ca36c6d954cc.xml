  <chunk id="49ef14fbeee682057ed14674f623ca36c6d954cc.c0" text="RNA and DNA Bacteriophages as Molecular Diagnosis Controls in Clinical Virology: A Comprehensive Study of More than 45,000 Routine PCR Tests">
    <entity charOffset="0-3" id="49ef14fbeee682057ed14674f623ca36c6d954cc.c0.e0" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
    <entity charOffset="8-11" id="49ef14fbeee682057ed14674f623ca36c6d954cc.c0.e1" ontology_id="CHEBI_16991" text="DNA" type="chemical"/>
