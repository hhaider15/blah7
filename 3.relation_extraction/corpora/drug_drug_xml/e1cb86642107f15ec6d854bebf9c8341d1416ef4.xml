  <chunk id="e1cb86642107f15ec6d854bebf9c8341d1416ef4.c0" text="Strand-Exchange Nucleic Acid Circuitry with Enhanced Thermo-and Structure-Buffering Abilities Turns Gene Diagnostics Ultra-Reliable and Environmental Compatible OPEN">
    <entity charOffset="16-28" id="e1cb86642107f15ec6d854bebf9c8341d1416ef4.c0.e0" ontology_id="CHEBI_33696" text="Nucleic Acid" type="chemical"/>
