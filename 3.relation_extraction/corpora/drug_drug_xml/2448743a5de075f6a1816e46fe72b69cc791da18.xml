  <chunk id="2448743a5de075f6a1816e46fe72b69cc791da18.c0" text="Expression of 9-O-and 7,9-O-Acetyl Modified Sialic Acid in Cells and Their Effects on Influenza Viruses">
    <entity charOffset="44-55" id="2448743a5de075f6a1816e46fe72b69cc791da18.c0.e0" ontology_id="CHEBI_26667" text="Sialic Acid" type="chemical"/>
    <entity charOffset="51-55" id="2448743a5de075f6a1816e46fe72b69cc791da18.c0.e1" ontology_id="CHEBI_37527" text="Acid" type="chemical"/>
    <pair e1="2448743a5de075f6a1816e46fe72b69cc791da18.c0.e0" e2="2448743a5de075f6a1816e46fe72b69cc791da18.c0.e1" id="2448743a5de075f6a1816e46fe72b69cc791da18.c0.p0" relation="true"/>
