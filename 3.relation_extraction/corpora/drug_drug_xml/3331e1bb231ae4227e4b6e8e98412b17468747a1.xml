  <chunk id="3331e1bb231ae4227e4b6e8e98412b17468747a1.c0" text="Protein co-expression network analysis (ProCoNA)">
    <entity charOffset="0-7" id="3331e1bb231ae4227e4b6e8e98412b17468747a1.c0.e0" ontology_id="CHEBI_16541" text="Protein" type="chemical"/>
