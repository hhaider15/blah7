  <chunk id="c916747a33f9b4a986dd0e4a4b853db6814b74f9.c0" text="A General Strategy to Endow Natural Fusion-protein- Derived Peptides with Potent Antiviral Activity">
    <entity charOffset="60-68" id="c916747a33f9b4a986dd0e4a4b853db6814b74f9.c0.e0" ontology_id="CHEBI_16670" text="Peptides" type="chemical"/>
