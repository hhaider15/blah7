  <chunk id="540a2a909e0ae17de908c236742379d073ab2d95.c0" text="Evaluation of Three Automated Nucleic Acid Extraction Systems for Identification of Respiratory Viruses in Clinical Specimens by Multiplex Real-Time PCR">
    <entity charOffset="30-42" id="540a2a909e0ae17de908c236742379d073ab2d95.c0.e0" ontology_id="CHEBI_33696" text="Nucleic Acid" type="chemical"/>
    <entity charOffset="38-42" id="540a2a909e0ae17de908c236742379d073ab2d95.c0.e1" ontology_id="CHEBI_37527" text="Acid" type="chemical"/>
    <pair e1="540a2a909e0ae17de908c236742379d073ab2d95.c0.e0" e2="540a2a909e0ae17de908c236742379d073ab2d95.c0.e1" id="540a2a909e0ae17de908c236742379d073ab2d95.c0.p0" relation="true"/>
