  <chunk id="295d1050a0d5e811698a853c29cc8b388d4d92f7.c0" text="The impact of the interferon/TNF- Related Apoptosis-inducing Ligand Signaling Axis on Disease Progression in Respiratory viral infection and Beyond">
    <entity charOffset="61-67" id="295d1050a0d5e811698a853c29cc8b388d4d92f7.c0.e0" ontology_id="CHEBI_52214" text="Ligand" type="chemical"/>
