  <chunk id="e782e045471faad6e0a63dae1f064e345d754493.c0" text="Identification of a Conserved Non-Protein-Coding Genomic Element that Plays an Essential Role in Alphabaculovirus Pathogenesis">
    <entity charOffset="57-64" id="e782e045471faad6e0a63dae1f064e345d754493.c0.e0" ontology_id="CHEBI_33250" text="Element" type="chemical"/>
