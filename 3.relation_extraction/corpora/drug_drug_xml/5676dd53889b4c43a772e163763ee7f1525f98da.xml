  <chunk id="5676dd53889b4c43a772e163763ee7f1525f98da.c0" text="molecules Separation and Quantification of Four Main Chiral Glucosinolates in Radix Isatidis and Its Granules Using High-Performance Liquid Chromatography/Diode Array Detector Coupled with Circular Dichroism Detection">
    <entity charOffset="0-9" id="5676dd53889b4c43a772e163763ee7f1525f98da.c0.e0" ontology_id="CHEBI_25367" text="molecules" type="chemical"/>
    <entity charOffset="60-74" id="5676dd53889b4c43a772e163763ee7f1525f98da.c0.e1" ontology_id="CHEBI_24279" text="Glucosinolates" type="chemical"/>
    <pair e1="5676dd53889b4c43a772e163763ee7f1525f98da.c0.e0" e2="5676dd53889b4c43a772e163763ee7f1525f98da.c0.e1" id="5676dd53889b4c43a772e163763ee7f1525f98da.c0.p0" relation="true"/>
