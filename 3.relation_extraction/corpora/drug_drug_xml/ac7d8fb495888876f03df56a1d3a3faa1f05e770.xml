  <chunk id="ac7d8fb495888876f03df56a1d3a3faa1f05e770.c0" text="Inhibition of the Hantavirus Fusion Process by Predicted Domain III and Stem Peptides from Glycoprotein Gc">
    <entity charOffset="77-85" id="ac7d8fb495888876f03df56a1d3a3faa1f05e770.c0.e0" ontology_id="CHEBI_16670" text="Peptides" type="chemical"/>
