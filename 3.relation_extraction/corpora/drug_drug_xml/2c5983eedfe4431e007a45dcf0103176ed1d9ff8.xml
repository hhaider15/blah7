  <chunk id="2c5983eedfe4431e007a45dcf0103176ed1d9ff8.c0" text="Identification of Three Antiviral Inhibitors against Japanese Encephalitis Virus from Library of Pharmacologically Active Compounds 1280">
    <entity charOffset="24-33" id="2c5983eedfe4431e007a45dcf0103176ed1d9ff8.c0.e0" ontology_id="CHEBI_22587" text="Antiviral" type="chemical"/>
