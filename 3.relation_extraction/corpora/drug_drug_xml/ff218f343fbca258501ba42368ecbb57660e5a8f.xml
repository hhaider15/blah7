  <chunk id="ff218f343fbca258501ba42368ecbb57660e5a8f.c0" text="An Analogue of the Antibiotic Teicoplanin Prevents Flavivirus Entry In Vitro">
    <entity charOffset="19-29" id="ff218f343fbca258501ba42368ecbb57660e5a8f.c0.e0" ontology_id="CHEBI_33281" text="Antibiotic" type="chemical"/>
    <entity charOffset="30-41" id="ff218f343fbca258501ba42368ecbb57660e5a8f.c0.e1" ontology_id="CHEBI_29687" text="Teicoplanin" type="chemical"/>
    <pair e1="ff218f343fbca258501ba42368ecbb57660e5a8f.c0.e0" e2="ff218f343fbca258501ba42368ecbb57660e5a8f.c0.e1" id="ff218f343fbca258501ba42368ecbb57660e5a8f.c0.p0" relation="true"/>
