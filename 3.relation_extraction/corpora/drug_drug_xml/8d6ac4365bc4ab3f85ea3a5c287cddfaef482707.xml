  <chunk id="8d6ac4365bc4ab3f85ea3a5c287cddfaef482707.c0" text="Comparison of Infectious Agents Susceptibility to Photocatalytic Effects of Nanosized Titanium and Zinc Oxides: A Practical Approach">
    <entity charOffset="86-94" id="8d6ac4365bc4ab3f85ea3a5c287cddfaef482707.c0.e0" ontology_id="CHEBI_33341" text="Titanium" type="chemical"/>
    <entity charOffset="99-103" id="8d6ac4365bc4ab3f85ea3a5c287cddfaef482707.c0.e1" ontology_id="CHEBI_27363" text="Zinc" type="chemical"/>
