  <chunk id="8485271eeb378baa284392c0d7174a5612463048.c0" text="MINI REVIEW Use of Peptides for the Management of Alzheimer's Disease: Diagnosis and Inhibition">
    <entity charOffset="19-27" id="8485271eeb378baa284392c0d7174a5612463048.c0.e0" ontology_id="CHEBI_16670" text="Peptides" type="chemical"/>
