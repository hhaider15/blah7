  <chunk id="65c5b049ad6a45d3533e0fee55018ba631d338b0.c0" text="The enemy within: Targeting host-parasite interaction for antileishmanial drug discovery">
    <entity charOffset="58-78" id="65c5b049ad6a45d3533e0fee55018ba631d338b0.c0.e0" ontology_id="CHEBI_70868" text="antileishmanial drug" type="chemical"/>
    <entity charOffset="74-78" id="65c5b049ad6a45d3533e0fee55018ba631d338b0.c0.e1" ontology_id="CHEBI_23888" text="drug" type="chemical"/>
    <pair e1="65c5b049ad6a45d3533e0fee55018ba631d338b0.c0.e0" e2="65c5b049ad6a45d3533e0fee55018ba631d338b0.c0.e1" id="65c5b049ad6a45d3533e0fee55018ba631d338b0.c0.p0" relation="true"/>
