  <chunk id="945fd47a82b49774675d732d27f6b806c0e6aeb1.c0" text="Nonhuman Primate IFITM Proteins Are Potent Inhibitors of HIV and SIV">
    <entity charOffset="23-31" id="945fd47a82b49774675d732d27f6b806c0e6aeb1.c0.e0" ontology_id="CHEBI_36080" text="Proteins" type="chemical"/>
