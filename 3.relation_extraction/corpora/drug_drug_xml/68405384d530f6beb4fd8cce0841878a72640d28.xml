  <chunk id="68405384d530f6beb4fd8cce0841878a72640d28.c0" text="New Antimicrobial Potential and Structural Properties of PAFB: A Cationic, Cysteine-Rich Protein from Penicillium chrysogenum Q176">
    <entity charOffset="4-17" id="68405384d530f6beb4fd8cce0841878a72640d28.c0.e0" ontology_id="CHEBI_33281" text="Antimicrobial" type="chemical"/>
