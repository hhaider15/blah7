  <chunk id="097b9b4ada3e89942ea196c59226e95da1b7969f.c0" text="Oral vitamin A supplementation of porcine epidemic diarrhea virus infected gilts enhances IgA and lactogenic immune protection of nursing piglets">
    <entity charOffset="5-12" id="097b9b4ada3e89942ea196c59226e95da1b7969f.c0.e0" ontology_id="CHEBI_33229" text="vitamin" type="chemical"/>
    <entity charOffset="5-14" id="097b9b4ada3e89942ea196c59226e95da1b7969f.c0.e1" ontology_id="CHEBI_12777" text="vitamin A" type="chemical"/>
    <pair e1="097b9b4ada3e89942ea196c59226e95da1b7969f.c0.e0" e2="097b9b4ada3e89942ea196c59226e95da1b7969f.c0.e1" id="097b9b4ada3e89942ea196c59226e95da1b7969f.c0.p0" relation="true"/>
