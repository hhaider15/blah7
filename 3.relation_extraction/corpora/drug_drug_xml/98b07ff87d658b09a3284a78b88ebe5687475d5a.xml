  <chunk id="98b07ff87d658b09a3284a78b88ebe5687475d5a.c0" text="BMC Immunology HLA class I supertypes: a revised and updated classification">
    <entity charOffset="0-3" id="98b07ff87d658b09a3284a78b88ebe5687475d5a.c0.e0" ontology_id="CHEBI_3392" text="BMC" type="chemical"/>
