  <chunk id="c9b0389a55de2f9cbfe37049d1072e0984613923.c0" text="Elevation of Intact and Proteolytic Fragments of Acute Phase Proteins Constitutes the Earliest Systemic Antiviral Response in HIV-1 Infection">
    <entity charOffset="61-69" id="c9b0389a55de2f9cbfe37049d1072e0984613923.c0.e0" ontology_id="CHEBI_36080" text="Proteins" type="chemical"/>
    <entity charOffset="104-113" id="c9b0389a55de2f9cbfe37049d1072e0984613923.c0.e1" ontology_id="CHEBI_22587" text="Antiviral" type="chemical"/>
    <pair e1="c9b0389a55de2f9cbfe37049d1072e0984613923.c0.e0" e2="c9b0389a55de2f9cbfe37049d1072e0984613923.c0.e1" id="c9b0389a55de2f9cbfe37049d1072e0984613923.c0.p0" relation="true"/>
