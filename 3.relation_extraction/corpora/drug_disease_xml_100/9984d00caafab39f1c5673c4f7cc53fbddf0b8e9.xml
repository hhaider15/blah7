  <chunk id="9984d00caafab39f1c5673c4f7cc53fbddf0b8e9.c0" text="A G1-lineage H9N2 virus with oviduct tropism causes chronic pathological changes in the infundibulum and a long-lasting drop in egg production">
    <entity charOffset="37-44" id="9984d00caafab39f1c5673c4f7cc53fbddf0b8e9.c0.e0" ontology_id="GO_0009606" text="tropism" type="gene_function"/>
