  <chunk id="98ce22d0a0cab728f1aab48170f1769bb0bc3454.c0" text="Population-and Variant-Based Genome Analyses of Viruses from Vaccine-Derived Rabies Cases Demonstrate Product Specific Clusters and Unique Patterns">
    <entity charOffset="77-83" id="98ce22d0a0cab728f1aab48170f1769bb0bc3454.c0.e0" ontology_id="DOID_11260" text="Rabies" type="disease"/>
    <entity charOffset="119-127" id="98ce22d0a0cab728f1aab48170f1769bb0bc3454.c0.e1" ontology_id="CHEBI_33731" text="Clusters" type="chemical"/>
    <pair e1="98ce22d0a0cab728f1aab48170f1769bb0bc3454.c0.e0" e2="98ce22d0a0cab728f1aab48170f1769bb0bc3454.c0.e1" id="98ce22d0a0cab728f1aab48170f1769bb0bc3454.c0.p0" relation="true"/>
