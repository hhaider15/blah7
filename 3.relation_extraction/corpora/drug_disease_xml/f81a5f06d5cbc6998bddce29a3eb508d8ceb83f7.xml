  <chunk id="f81a5f06d5cbc6998bddce29a3eb508d8ceb83f7.c0" text="Angiotensin-converting enzyme 2 is reduced in Alzheimer's disease in association with increasing amyloid-β and tau pathology">
    <entity charOffset="58-65" id="f81a5f06d5cbc6998bddce29a3eb508d8ceb83f7.c0.e0" ontology_id="DOID_4" text="disease" type="disease"/>
