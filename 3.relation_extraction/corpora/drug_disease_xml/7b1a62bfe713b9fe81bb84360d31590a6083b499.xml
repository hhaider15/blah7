  <chunk id="7b1a62bfe713b9fe81bb84360d31590a6083b499.c0" text="A Functional Role of Fibroblast Growth Factor Receptor 1 (FGFR1) in the Suppression of Influenza A Virus Replication">
    <entity charOffset="13-17" id="7b1a62bfe713b9fe81bb84360d31590a6083b499.c0.e0" ontology_id="CHEBI_50906" text="Role" type="chemical"/>
    <entity charOffset="87-96" id="7b1a62bfe713b9fe81bb84360d31590a6083b499.c0.e1" ontology_id="DOID_8469" text="Influenza" type="disease"/>
    <pair e1="7b1a62bfe713b9fe81bb84360d31590a6083b499.c0.e0" e2="7b1a62bfe713b9fe81bb84360d31590a6083b499.c0.e1" id="7b1a62bfe713b9fe81bb84360d31590a6083b499.c0.p0" relation="true"/>
