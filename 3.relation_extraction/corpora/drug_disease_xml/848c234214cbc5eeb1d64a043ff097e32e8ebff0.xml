  <chunk id="848c234214cbc5eeb1d64a043ff097e32e8ebff0.c0" text="Suppression and resolution of autoimmune arthritis by rhesus θ-defensin-1, an immunomodulatory macrocyclic peptide">
    <entity charOffset="41-50" id="848c234214cbc5eeb1d64a043ff097e32e8ebff0.c0.e0" ontology_id="DOID_848" text="arthritis" type="disease"/>
