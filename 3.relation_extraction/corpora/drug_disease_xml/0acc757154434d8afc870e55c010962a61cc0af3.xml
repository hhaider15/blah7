  <chunk id="0acc757154434d8afc870e55c010962a61cc0af3.c0" text="Internal control for real-time polymerase chain reaction based on MS2 bacteriophage for RNA viruses diagnostics">
    <entity charOffset="88-91" id="0acc757154434d8afc870e55c010962a61cc0af3.c0.e0" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
