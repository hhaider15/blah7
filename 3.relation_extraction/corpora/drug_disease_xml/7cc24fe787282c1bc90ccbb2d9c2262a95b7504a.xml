  <chunk id="7cc24fe787282c1bc90ccbb2d9c2262a95b7504a.c0" text="Distinct Regulation of Host Responses by ERK and JNK MAP Kinases in Swine Macrophages Infected with Pandemic (H1N1) 2009 Influenza Virus">
    <entity charOffset="53-56" id="7cc24fe787282c1bc90ccbb2d9c2262a95b7504a.c0.e0" ontology_id="CHEBI_6716" text="MAP" type="chemical"/>
    <entity charOffset="121-130" id="7cc24fe787282c1bc90ccbb2d9c2262a95b7504a.c0.e1" ontology_id="DOID_8469" text="Influenza" type="disease"/>
    <pair e1="7cc24fe787282c1bc90ccbb2d9c2262a95b7504a.c0.e0" e2="7cc24fe787282c1bc90ccbb2d9c2262a95b7504a.c0.e1" id="7cc24fe787282c1bc90ccbb2d9c2262a95b7504a.c0.p0" relation="true"/>
