  <chunk id="f3b7f4469ac01f1ce916d24172570c43c537627e.c0" text="Glycyrrhizin Exerts Antioxidative Effects in H5N1 Influenza A Virus-Infected Cells and Inhibits Virus Replication and Pro-Inflammatory Gene Expression">
    <entity charOffset="0-12" id="f3b7f4469ac01f1ce916d24172570c43c537627e.c0.e0" ontology_id="CHEBI_15939" text="Glycyrrhizin" type="chemical"/>
    <entity charOffset="50-59" id="f3b7f4469ac01f1ce916d24172570c43c537627e.c0.e1" ontology_id="DOID_8469" text="Influenza" type="disease"/>
    <pair e1="f3b7f4469ac01f1ce916d24172570c43c537627e.c0.e0" e2="f3b7f4469ac01f1ce916d24172570c43c537627e.c0.e1" id="f3b7f4469ac01f1ce916d24172570c43c537627e.c0.p0" relation="true"/>
