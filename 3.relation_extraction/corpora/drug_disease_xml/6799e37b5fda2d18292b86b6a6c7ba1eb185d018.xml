  <chunk id="6799e37b5fda2d18292b86b6a6c7ba1eb185d018.c0" text="Late Endosomal/Lysosomal Cholesterol Accumulation Is a Host Cell-Protective Mechanism Inhibiting Endosomal Escape of Influenza A Virus">
    <entity charOffset="25-36" id="6799e37b5fda2d18292b86b6a6c7ba1eb185d018.c0.e0" ontology_id="CHEBI_16113" text="Cholesterol" type="chemical"/>
