  <chunk id="0867cf1922a634eefb3a3ad07f4dc6c9c94828ea.c0" text="High Throughput Screening for Small Molecule Enhancers of the Interferon Signaling Pathway to Drive Next-Generation Antiviral Drug Discovery">
    <entity charOffset="36-44" id="0867cf1922a634eefb3a3ad07f4dc6c9c94828ea.c0.e0" ontology_id="CHEBI_25367" text="Molecule" type="chemical"/>
