  <chunk id="27f21a68a6f074f434e506aee9492fa2c772847c.c0" text="Novel protein chip for the detection of antibodies against infectious bronchitis virus">
    <entity charOffset="6-13" id="27f21a68a6f074f434e506aee9492fa2c772847c.c0.e0" ontology_id="CHEBI_16541" text="protein" type="chemical"/>
    <entity charOffset="70-80" id="27f21a68a6f074f434e506aee9492fa2c772847c.c0.e1" ontology_id="DOID_6132" text="bronchitis" type="disease"/>
    <pair e1="27f21a68a6f074f434e506aee9492fa2c772847c.c0.e0" e2="27f21a68a6f074f434e506aee9492fa2c772847c.c0.e1" id="27f21a68a6f074f434e506aee9492fa2c772847c.c0.p0" relation="true"/>
