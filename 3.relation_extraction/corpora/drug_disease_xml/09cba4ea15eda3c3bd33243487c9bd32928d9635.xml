  <chunk id="09cba4ea15eda3c3bd33243487c9bd32928d9635.c0" text="Identification of Secreted Proteins Involved in Nonspecific dsRNA-Mediated Lutzomyia longipalpis LL5 Cell Antiviral Response">
    <entity charOffset="27-35" id="09cba4ea15eda3c3bd33243487c9bd32928d9635.c0.e0" ontology_id="CHEBI_36080" text="Proteins" type="chemical"/>
    <entity charOffset="106-115" id="09cba4ea15eda3c3bd33243487c9bd32928d9635.c0.e1" ontology_id="CHEBI_22587" text="Antiviral" type="chemical"/>
