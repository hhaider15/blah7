  <chunk id="b453be7df859d451667f709fbb0c843cf0b9f25c.c0" text="Biguanide is a modifiable pharmacophore for recruitment of endogenous Zn 2+ to inhibit cysteinyl cathepsins: review and implications">
    <entity charOffset="0-9" id="b453be7df859d451667f709fbb0c843cf0b9f25c.c0.e0" ontology_id="CHEBI_3095" text="Biguanide" type="chemical"/>
