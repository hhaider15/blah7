  <chunk id="69718b1722e6fb921fa5c56b5135454ddbdf7457.c0" text="KDELR2 Competes with Measles Virus Envelope Proteins for Cellular Chaperones Reducing Their Chaperone-Mediated Cell Surface Transport">
    <entity charOffset="21-28" id="69718b1722e6fb921fa5c56b5135454ddbdf7457.c0.e0" ontology_id="DOID_8622" text="Measles" type="disease"/>
