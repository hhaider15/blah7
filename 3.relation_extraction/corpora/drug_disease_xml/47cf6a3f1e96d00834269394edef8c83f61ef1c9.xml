  <chunk id="47cf6a3f1e96d00834269394edef8c83f61ef1c9.c1" text="We aimed to describe patterns in the usage of antivirals to treat influenza virus infection in hospitals in Hong Kong from 2000 through 2015.">
    <entity charOffset="46-56" id="47cf6a3f1e96d00834269394edef8c83f61ef1c9.c1.e0" ontology_id="CHEBI_22587" text="antivirals" type="chemical"/>
