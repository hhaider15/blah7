  <chunk id="42145eff1cc724f8e5ad1a8e04cc421512d8ec7a.c0" text="S1 Subunit of Spike Protein from a Current Highly Virulent Porcine Epidemic Diarrhea Virus Is an Important Determinant of Virulence in Piglets">
    <entity charOffset="20-27" id="42145eff1cc724f8e5ad1a8e04cc421512d8ec7a.c0.e0" ontology_id="CHEBI_16541" text="Protein" type="chemical"/>
    <entity charOffset="76-84" id="42145eff1cc724f8e5ad1a8e04cc421512d8ec7a.c0.e1" ontology_id="DOID_13250" text="Diarrhea" type="disease"/>
    <pair e1="42145eff1cc724f8e5ad1a8e04cc421512d8ec7a.c0.e0" e2="42145eff1cc724f8e5ad1a8e04cc421512d8ec7a.c0.e1" id="42145eff1cc724f8e5ad1a8e04cc421512d8ec7a.c0.p0" relation="true"/>
