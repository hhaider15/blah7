  <chunk id="d94115064a8168415d634f654869a4009e4c3c70.c0" text="Preliminary Findings of a Randomized Trial of Non- Pharmaceutical Interventions to Prevent Influenza Transmission in Households">
    <entity charOffset="51-65" id="d94115064a8168415d634f654869a4009e4c3c70.c0.e0" ontology_id="CHEBI_52217" text="Pharmaceutical" type="chemical"/>
    <entity charOffset="91-100" id="d94115064a8168415d634f654869a4009e4c3c70.c0.e1" ontology_id="DOID_8469" text="Influenza" type="disease"/>
    <pair e1="d94115064a8168415d634f654869a4009e4c3c70.c0.e0" e2="d94115064a8168415d634f654869a4009e4c3c70.c0.e1" id="d94115064a8168415d634f654869a4009e4c3c70.c0.p0" relation="true"/>
