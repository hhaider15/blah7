  <chunk id="4c71e5b4d4b332ee8522373d949ceff2bed03718.c0" text="Expression and regulation of the neutral amino acid transporter B 0 AT1 in rat small intestine">
    <entity charOffset="41-46" id="4c71e5b4d4b332ee8522373d949ceff2bed03718.c0.e0" ontology_id="CHEBI_46882" text="amino" type="chemical"/>
    <entity charOffset="41-51" id="4c71e5b4d4b332ee8522373d949ceff2bed03718.c0.e1" ontology_id="CHEBI_33704" text="amino acid" type="chemical"/>
    <entity charOffset="47-51" id="4c71e5b4d4b332ee8522373d949ceff2bed03718.c0.e2" ontology_id="CHEBI_37527" text="acid" type="chemical"/>
