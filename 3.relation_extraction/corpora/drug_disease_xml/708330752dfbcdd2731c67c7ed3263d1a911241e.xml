  <chunk id="708330752dfbcdd2731c67c7ed3263d1a911241e.c0" text="Feline Foamy Virus Infection: Characterization of Experimental Infection and Prevalence of Natural Infection in Domestic Cats with and without Chronic Kidney Disease">
    <entity charOffset="143-165" id="708330752dfbcdd2731c67c7ed3263d1a911241e.c0.e0" ontology_id="DOID_784" text="Chronic Kidney Disease" type="disease"/>
