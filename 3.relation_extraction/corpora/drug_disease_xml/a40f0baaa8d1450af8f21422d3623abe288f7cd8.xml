  <chunk id="a40f0baaa8d1450af8f21422d3623abe288f7cd8.c0" text="molecules Antimicrobial and Antioxidant Activities of Essential Oils of Satureja thymbra Growing Wild in Libya">
    <entity charOffset="0-9" id="a40f0baaa8d1450af8f21422d3623abe288f7cd8.c0.e0" ontology_id="CHEBI_25367" text="molecules" type="chemical"/>
