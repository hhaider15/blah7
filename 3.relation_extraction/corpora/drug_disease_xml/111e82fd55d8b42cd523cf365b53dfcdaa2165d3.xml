  <chunk id="111e82fd55d8b42cd523cf365b53dfcdaa2165d3.c0" text="A Recombinant La Sota Vaccine Strain Expressing Multiple Epitopes of Infectious Bronchitis Virus (IBV) Protects Specific Pathogen-Free (SPF) Chickens against IBV and NDV Challenges">
    <entity charOffset="80-90" id="111e82fd55d8b42cd523cf365b53dfcdaa2165d3.c0.e0" ontology_id="DOID_6132" text="Bronchitis" type="disease"/>
    <entity charOffset="136-139" id="111e82fd55d8b42cd523cf365b53dfcdaa2165d3.c0.e1" ontology_id="CHEBI_88542" text="SPF" type="chemical"/>
    <pair e1="111e82fd55d8b42cd523cf365b53dfcdaa2165d3.c0.e0" e2="111e82fd55d8b42cd523cf365b53dfcdaa2165d3.c0.e1" id="111e82fd55d8b42cd523cf365b53dfcdaa2165d3.c0.p0" relation="true"/>
