  <chunk id="78233b5cc660bff6f716fa6cbb967775bf5c7417.c0" text="Bat lung epithelial cells show greater host species-specific innate resistance than MDCK cells to human and avian influenza viruses">
    <entity charOffset="108-123" id="78233b5cc660bff6f716fa6cbb967775bf5c7417.c0.e0" ontology_id="DOID_4492" text="avian influenza" type="disease"/>
    <entity charOffset="114-123" id="78233b5cc660bff6f716fa6cbb967775bf5c7417.c0.e1" ontology_id="DOID_8469" text="influenza" type="disease"/>
  </chunk>
  <chunk id="78233b5cc660bff6f716fa6cbb967775bf5c7417.c1" text="Background: With the recent discovery of novel H17N10 and H18N11 influenza viral RNA in bats and report on high frequency of avian H9 seroconversion in a species of free ranging bats, an important issue to address is the extent bats are susceptible to conventional avian and human influenza A viruses.">
    <entity charOffset="65-74" id="78233b5cc660bff6f716fa6cbb967775bf5c7417.c1.e0" ontology_id="DOID_8469" text="influenza" type="disease"/>
    <entity charOffset="81-84" id="78233b5cc660bff6f716fa6cbb967775bf5c7417.c1.e1" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
