  <chunk id="2ad8032804b3deb79b634cfbad52efbf2f90da76.c0" text="molecules Antiviral Activity of Novel Quinoline Derivatives against Dengue Virus Serotype 2">
    <entity charOffset="0-9" id="2ad8032804b3deb79b634cfbad52efbf2f90da76.c0.e0" ontology_id="CHEBI_25367" text="molecules" type="chemical"/>
