  <chunk id="e63eaf2a9904a7212185d87824ddd187ab110272.c0" text="BMC Microbiology The expression and antigenicity of a truncated spike-nucleocapsid fusion protein of severe acute respiratory syndrome-associated coronavirus">
    <entity charOffset="0-3" id="e63eaf2a9904a7212185d87824ddd187ab110272.c0.e0" ontology_id="CHEBI_3392" text="BMC" type="chemical"/>
    <entity charOffset="90-97" id="e63eaf2a9904a7212185d87824ddd187ab110272.c0.e1" ontology_id="CHEBI_16541" text="protein" type="chemical"/>
