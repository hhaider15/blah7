  <chunk id="9718efa7298b28b740938eeca415b1703ec6f689.c0" text="Effects of ribavirin on the replication and genetic stability of porcine reproductive and respiratory syndrome virus">
    <entity charOffset="11-20" id="9718efa7298b28b740938eeca415b1703ec6f689.c0.e0" ontology_id="CHEBI_63580" text="ribavirin" type="chemical"/>
