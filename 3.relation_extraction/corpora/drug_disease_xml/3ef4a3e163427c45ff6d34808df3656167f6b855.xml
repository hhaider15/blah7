  <chunk id="3ef4a3e163427c45ff6d34808df3656167f6b855.c0" text="Dietary Quercetin Increases Colonic Microbial Diversity and Attenuates Colitis Severity in Citrobacter rodentium-Infected Mice">
    <entity charOffset="8-17" id="3ef4a3e163427c45ff6d34808df3656167f6b855.c0.e0" ontology_id="CHEBI_16243" text="Quercetin" type="chemical"/>
    <entity charOffset="71-78" id="3ef4a3e163427c45ff6d34808df3656167f6b855.c0.e1" ontology_id="DOID_0060180" text="Colitis" type="disease"/>
    <pair e1="3ef4a3e163427c45ff6d34808df3656167f6b855.c0.e0" e2="3ef4a3e163427c45ff6d34808df3656167f6b855.c0.e1" id="3ef4a3e163427c45ff6d34808df3656167f6b855.c0.p0" relation="true"/>
