  <chunk id="1c2e5c34faa703640900893c58254c63361bd29b.c0" text="Vaccinomics Approach for Designing Potential Peptide Vaccine by Targeting Shigella spp. Serine Protease Autotransporter Subfamily Protein SigA">
    <entity charOffset="45-52" id="1c2e5c34faa703640900893c58254c63361bd29b.c0.e0" ontology_id="CHEBI_16670" text="Peptide" type="chemical"/>
