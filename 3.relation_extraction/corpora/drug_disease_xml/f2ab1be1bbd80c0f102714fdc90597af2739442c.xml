  <chunk id="f2ab1be1bbd80c0f102714fdc90597af2739442c.c0" text="Comparative Efficacy of Hemagglutinin, Nucleoprotein, and Matrix 2 Protein Gene-Based Vaccination against H5N1 Influenza in Mouse and Ferret">
    <entity charOffset="67-74" id="f2ab1be1bbd80c0f102714fdc90597af2739442c.c0.e0" ontology_id="CHEBI_16541" text="Protein" type="chemical"/>
    <entity charOffset="111-120" id="f2ab1be1bbd80c0f102714fdc90597af2739442c.c0.e1" ontology_id="DOID_8469" text="Influenza" type="disease"/>
    <pair e1="f2ab1be1bbd80c0f102714fdc90597af2739442c.c0.e0" e2="f2ab1be1bbd80c0f102714fdc90597af2739442c.c0.e1" id="f2ab1be1bbd80c0f102714fdc90597af2739442c.c0.p0" relation="true"/>
