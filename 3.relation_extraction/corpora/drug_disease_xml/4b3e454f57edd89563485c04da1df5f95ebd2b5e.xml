  <chunk id="4b3e454f57edd89563485c04da1df5f95ebd2b5e.c0" text="SARS coronavirus protein 7a interacts with human Ap 4 A-hydrolase">
    <entity charOffset="0-4" id="4b3e454f57edd89563485c04da1df5f95ebd2b5e.c0.e0" ontology_id="DOID_2945" text="SARS" type="disease"/>
