  <chunk id="73f93646de4efa4c3b8d02d07dea684c028763d1.c0" text="Investigation of Pathogenesis of H1N1 Influenza Virus and Swine Streptococcus suis Serotype 2 Co-Infection in Pigs by Microarray Analysis a11111 OPEN ACCESS">
    <entity charOffset="38-47" id="73f93646de4efa4c3b8d02d07dea684c028763d1.c0.e0" ontology_id="DOID_8469" text="Influenza" type="disease"/>
