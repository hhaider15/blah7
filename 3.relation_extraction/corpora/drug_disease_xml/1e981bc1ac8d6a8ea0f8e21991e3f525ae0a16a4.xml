  <chunk id="1e981bc1ac8d6a8ea0f8e21991e3f525ae0a16a4.c0" text="RNA Sequencing of H3N2 Influenza Virus-Infected Human Nasal Epithelial Cells from Multiple Subjects Reveals Molecular Pathways Associated with Tissue Injury and Complications">
    <entity charOffset="0-3" id="1e981bc1ac8d6a8ea0f8e21991e3f525ae0a16a4.c0.e0" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
    <entity charOffset="23-32" id="1e981bc1ac8d6a8ea0f8e21991e3f525ae0a16a4.c0.e1" ontology_id="DOID_8469" text="Influenza" type="disease"/>
