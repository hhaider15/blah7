  <chunk id="9d82ed9e4eab5525576c427521c25aca625ecd59.c0" text="molecules Probing the Effects of Pyrimidine Functional Group Switches on Acyclic Fleximer Analogues for Antiviral Activity">
    <entity charOffset="0-9" id="9d82ed9e4eab5525576c427521c25aca625ecd59.c0.e0" ontology_id="CHEBI_25367" text="molecules" type="chemical"/>
