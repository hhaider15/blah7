  <chunk id="854e623d1f875e4605b2ffd3f72599d063a56cc0.c0" text="Diversity of Salmonella spp. serovars isolated from the intestines of water buffalo calves with gastroenteritis">
    <entity charOffset="70-75" id="854e623d1f875e4605b2ffd3f72599d063a56cc0.c0.e0" ontology_id="CHEBI_15377" text="water" type="chemical"/>
