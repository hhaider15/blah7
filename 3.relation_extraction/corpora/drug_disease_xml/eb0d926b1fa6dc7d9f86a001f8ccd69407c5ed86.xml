  <chunk id="eb0d926b1fa6dc7d9f86a001f8ccd69407c5ed86.c0" text="Overlapping Patterns of Rapid Evolution in the Nucleic Acid Sensors cGAS and OAS1 Suggest a Common Mechanism of Pathogen Antagonism and Escape">
    <entity charOffset="47-59" id="eb0d926b1fa6dc7d9f86a001f8ccd69407c5ed86.c0.e0" ontology_id="CHEBI_33696" text="Nucleic Acid" type="chemical"/>
    <entity charOffset="55-59" id="eb0d926b1fa6dc7d9f86a001f8ccd69407c5ed86.c0.e1" ontology_id="CHEBI_37527" text="Acid" type="chemical"/>
