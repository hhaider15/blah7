  <chunk id="d73d189618e61f04c6dbabf3bbf8aa7adc65b156.c0" text="Molecular Sciences Theranostics Aspects of Various Nanoparticles in Veterinary Medicine">
    <entity charOffset="51-64" id="d73d189618e61f04c6dbabf3bbf8aa7adc65b156.c0.e0" ontology_id="CHEBI_50803" text="Nanoparticles" type="chemical"/>
    <entity charOffset="79-87" id="d73d189618e61f04c6dbabf3bbf8aa7adc65b156.c0.e1" ontology_id="CHEBI_23888" text="Medicine" type="chemical"/>
