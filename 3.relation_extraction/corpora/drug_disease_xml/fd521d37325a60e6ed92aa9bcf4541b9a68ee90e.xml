  <chunk id="fd521d37325a60e6ed92aa9bcf4541b9a68ee90e.c0" text="Identification of CD8 + cytotoxic T lymphocyte epitopes from porcine reproductive and respiratory syndrome virus matrix protein in BALB/c mice">
    <entity charOffset="98-106" id="fd521d37325a60e6ed92aa9bcf4541b9a68ee90e.c0.e0" ontology_id="DOID_225" text="syndrome" type="disease"/>
