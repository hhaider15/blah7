  <chunk id="d3f7afa8b4d0f21b23ccb1135dec12356375f5cc.c0" text="Therapeutic Vaccination in Chronic Hepatitis B: Preclinical Studies in the Woodchuck">
    <entity charOffset="27-44" id="d3f7afa8b4d0f21b23ccb1135dec12356375f5cc.c0.e0" ontology_id="DOID_2237" text="Chronic Hepatitis" type="disease"/>
