  <chunk id="4753feb97a6db2b1172caa96791e23e96ac9d287.c0" text="A Randomized Double Blinded Placebo-Controlled Clinical Trial of a Probiotic or Metronidazole for Acute Canine Diarrhea">
    <entity charOffset="80-93" id="4753feb97a6db2b1172caa96791e23e96ac9d287.c0.e0" ontology_id="CHEBI_6909" text="Metronidazole" type="chemical"/>
