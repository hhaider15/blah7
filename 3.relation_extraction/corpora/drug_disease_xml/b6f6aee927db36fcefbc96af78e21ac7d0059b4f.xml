  <chunk id="b6f6aee927db36fcefbc96af78e21ac7d0059b4f.c0" text="Downregulation of monocytic differentiation via modulation of CD147 by 3-hydroxy-3- methylglutaryl coenzyme A reductase inhibitors">
    <entity charOffset="99-107" id="b6f6aee927db36fcefbc96af78e21ac7d0059b4f.c0.e0" ontology_id="CHEBI_23354" text="coenzyme" type="chemical"/>
