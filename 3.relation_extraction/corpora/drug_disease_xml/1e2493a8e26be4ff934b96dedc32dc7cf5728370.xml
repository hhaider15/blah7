  <chunk id="1e2493a8e26be4ff934b96dedc32dc7cf5728370.c0" text="Trigger factor assisted soluble expression of recombinant spike protein of porcine epidemic diarrhea virus in Escherichia coli">
    <entity charOffset="64-71" id="1e2493a8e26be4ff934b96dedc32dc7cf5728370.c0.e0" ontology_id="CHEBI_16541" text="protein" type="chemical"/>
    <entity charOffset="92-100" id="1e2493a8e26be4ff934b96dedc32dc7cf5728370.c0.e1" ontology_id="DOID_13250" text="diarrhea" type="disease"/>
    <pair e1="1e2493a8e26be4ff934b96dedc32dc7cf5728370.c0.e0" e2="1e2493a8e26be4ff934b96dedc32dc7cf5728370.c0.e1" id="1e2493a8e26be4ff934b96dedc32dc7cf5728370.c0.p0" relation="true"/>
