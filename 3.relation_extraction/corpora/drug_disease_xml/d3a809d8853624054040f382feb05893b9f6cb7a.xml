  <chunk id="d3a809d8853624054040f382feb05893b9f6cb7a.c0" text="marine drugs Dereplication by High-Performance Liquid Chromatography (HPLC) with Quadrupole-Time-of-Flight Mass Spectroscopy (qTOF-MS) and Antiviral Activities of Phlorotannins from Ecklonia cava">
    <entity charOffset="7-12" id="d3a809d8853624054040f382feb05893b9f6cb7a.c0.e0" ontology_id="CHEBI_23888" text="drugs" type="chemical"/>
