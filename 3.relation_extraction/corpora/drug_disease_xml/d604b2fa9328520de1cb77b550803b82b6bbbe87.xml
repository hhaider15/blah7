  <chunk id="d604b2fa9328520de1cb77b550803b82b6bbbe87.c0" text="Duplex Molecular Assay Intended for Point-of-Care Diagnosis of Influenza A/B Virus Infection">
    <entity charOffset="63-72" id="d604b2fa9328520de1cb77b550803b82b6bbbe87.c0.e0" ontology_id="DOID_8469" text="Influenza" type="disease"/>
