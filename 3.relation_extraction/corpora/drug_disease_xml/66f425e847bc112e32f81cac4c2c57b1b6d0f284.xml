  <chunk id="66f425e847bc112e32f81cac4c2c57b1b6d0f284.c0" text="Autoimmune Cholangitis in the SJL/J Mouse is Antigen Non-specific">
    <entity charOffset="11-22" id="66f425e847bc112e32f81cac4c2c57b1b6d0f284.c0.e0" ontology_id="DOID_9446" text="Cholangitis" type="disease"/>
