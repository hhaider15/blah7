  <chunk id="fb9e05516efdae3600796552393fd3e3c26cb3d5.c0" text="Clinical Medicine An Inverse Relationship between Hyperuricemia and Mortality in Patients Undergoing Continuous Ambulatory Peritoneal Dialysis">
    <entity charOffset="9-17" id="fb9e05516efdae3600796552393fd3e3c26cb3d5.c0.e0" ontology_id="CHEBI_23888" text="Medicine" type="chemical"/>
