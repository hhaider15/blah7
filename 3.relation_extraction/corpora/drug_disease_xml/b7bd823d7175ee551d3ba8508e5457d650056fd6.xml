  <chunk id="b7bd823d7175ee551d3ba8508e5457d650056fd6.c0" text="Acquisition of Cell-Cell Fusion Activity by Amino Acid Substitutions in Spike Protein Determines the Infectivity of a Coronavirus in Cultured Cells">
    <entity charOffset="44-49" id="b7bd823d7175ee551d3ba8508e5457d650056fd6.c0.e0" ontology_id="CHEBI_46882" text="Amino" type="chemical"/>
