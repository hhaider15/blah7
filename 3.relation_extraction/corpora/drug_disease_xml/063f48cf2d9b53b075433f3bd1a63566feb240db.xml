  <chunk id="063f48cf2d9b53b075433f3bd1a63566feb240db.c0" text="Spatial Analysis of the Distribution, Risk Factors and Access to Medical Resources of Patients with Hepatitis B in Shenzhen, China">
    <entity charOffset="55-61" id="063f48cf2d9b53b075433f3bd1a63566feb240db.c0.e0" ontology_id="CHEBI_34922" text="Access" type="chemical"/>
