  <chunk id="4994fa72322bbf19120592304d92629226948d8e.c0" text="Rapid Identification of Malaria Vaccine Candidates Based on a-Helical Coiled Coil Protein Motif">
    <entity charOffset="24-31" id="4994fa72322bbf19120592304d92629226948d8e.c0.e0" ontology_id="DOID_12365" text="Malaria" type="disease"/>
