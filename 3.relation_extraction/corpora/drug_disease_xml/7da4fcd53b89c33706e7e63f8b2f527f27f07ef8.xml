  <chunk id="7da4fcd53b89c33706e7e63f8b2f527f27f07ef8.c0" text="Characterization of plasma proteins in children of different Mycobacterium tuberculosis infection status using label-free quantitative proteomics">
    <entity charOffset="27-35" id="7da4fcd53b89c33706e7e63f8b2f527f27f07ef8.c0.e0" ontology_id="CHEBI_36080" text="proteins" type="chemical"/>
