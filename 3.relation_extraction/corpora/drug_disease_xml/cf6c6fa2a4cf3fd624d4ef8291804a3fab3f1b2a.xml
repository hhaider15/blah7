  <chunk id="cf6c6fa2a4cf3fd624d4ef8291804a3fab3f1b2a.c0" text="The SARS Coronavirus 3a Protein Causes Endoplasmic Reticulum Stress and Induces Ligand-Independent Downregulation of the Type 1 Interferon Receptor">
    <entity charOffset="4-8" id="cf6c6fa2a4cf3fd624d4ef8291804a3fab3f1b2a.c0.e0" ontology_id="DOID_2945" text="SARS" type="disease"/>
