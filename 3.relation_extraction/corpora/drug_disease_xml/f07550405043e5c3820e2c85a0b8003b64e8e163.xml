  <chunk id="f07550405043e5c3820e2c85a0b8003b64e8e163.c0" text="Cleavage of the SARS Coronavirus Spike Glycoprotein by Airway Proteases Enhances Virus Entry into Human Bronchial Epithelial Cells In Vitro">
    <entity charOffset="16-20" id="f07550405043e5c3820e2c85a0b8003b64e8e163.c0.e0" ontology_id="DOID_2945" text="SARS" type="disease"/>
    <entity charOffset="39-51" id="f07550405043e5c3820e2c85a0b8003b64e8e163.c0.e1" ontology_id="CHEBI_17089" text="Glycoprotein" type="chemical"/>
