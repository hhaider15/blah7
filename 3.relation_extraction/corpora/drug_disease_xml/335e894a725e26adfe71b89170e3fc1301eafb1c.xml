  <chunk id="335e894a725e26adfe71b89170e3fc1301eafb1c.c0" text="Dissection of the Influenza A Virus Endocytic Routes Reveals Macropinocytosis as an Alternative Entry Pathway">
    <entity charOffset="18-27" id="335e894a725e26adfe71b89170e3fc1301eafb1c.c0.e0" ontology_id="DOID_8469" text="Influenza" type="disease"/>
