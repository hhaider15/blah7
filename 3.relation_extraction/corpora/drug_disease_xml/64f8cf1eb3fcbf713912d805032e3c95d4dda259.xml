  <chunk id="64f8cf1eb3fcbf713912d805032e3c95d4dda259.c0" text="Immunological properties of gold nanoparticles">
    <entity charOffset="28-32" id="64f8cf1eb3fcbf713912d805032e3c95d4dda259.c0.e0" ontology_id="CHEBI_29287" text="gold" type="chemical"/>
    <entity charOffset="33-46" id="64f8cf1eb3fcbf713912d805032e3c95d4dda259.c0.e1" ontology_id="CHEBI_50803" text="nanoparticles" type="chemical"/>
