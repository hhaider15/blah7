  <chunk id="5eb39003823f5339573f8f9ed82eaf7763feda68.c0" text="Manipulation of the Porcine Epidemic Diarrhea Virus Genome Using Targeted RNA Recombination">
    <entity charOffset="37-45" id="5eb39003823f5339573f8f9ed82eaf7763feda68.c0.e0" ontology_id="DOID_13250" text="Diarrhea" type="disease"/>
    <entity charOffset="74-77" id="5eb39003823f5339573f8f9ed82eaf7763feda68.c0.e1" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
    <pair e1="5eb39003823f5339573f8f9ed82eaf7763feda68.c0.e0" e2="5eb39003823f5339573f8f9ed82eaf7763feda68.c0.e1" id="5eb39003823f5339573f8f9ed82eaf7763feda68.c0.p0" relation="true"/>
