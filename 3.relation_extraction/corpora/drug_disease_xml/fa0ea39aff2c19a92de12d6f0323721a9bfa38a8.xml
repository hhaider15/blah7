  <chunk id="fa0ea39aff2c19a92de12d6f0323721a9bfa38a8.c0" text="Characterization of antiviral T cell responses during primary and secondary challenge of laboratory cats with feline infectious peritonitis virus (FIPV)">
    <entity charOffset="20-29" id="fa0ea39aff2c19a92de12d6f0323721a9bfa38a8.c0.e0" ontology_id="CHEBI_22587" text="antiviral" type="chemical"/>
