  <chunk id="dd9a2b263b1b66db904ed8a18dd6eba55e64bfff.c0" text="The Rational Design of Therapeutic Peptides for Aminopeptidase N using a Substrate-Based Approach OPEN">
    <entity charOffset="35-43" id="dd9a2b263b1b66db904ed8a18dd6eba55e64bfff.c0.e0" ontology_id="CHEBI_16670" text="Peptides" type="chemical"/>
