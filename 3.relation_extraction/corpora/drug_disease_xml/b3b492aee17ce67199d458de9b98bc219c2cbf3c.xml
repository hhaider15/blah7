  <chunk id="b3b492aee17ce67199d458de9b98bc219c2cbf3c.c0" text="Estrogen Mediates Innate and Adaptive Immune Alterations to Influenza Infection in Pregnant Mice">
    <entity charOffset="0-8" id="b3b492aee17ce67199d458de9b98bc219c2cbf3c.c0.e0" ontology_id="CHEBI_50114" text="Estrogen" type="chemical"/>
    <entity charOffset="60-69" id="b3b492aee17ce67199d458de9b98bc219c2cbf3c.c0.e1" ontology_id="DOID_8469" text="Influenza" type="disease"/>
    <pair e1="b3b492aee17ce67199d458de9b98bc219c2cbf3c.c0.e0" e2="b3b492aee17ce67199d458de9b98bc219c2cbf3c.c0.e1" id="b3b492aee17ce67199d458de9b98bc219c2cbf3c.c0.p0" relation="true"/>
