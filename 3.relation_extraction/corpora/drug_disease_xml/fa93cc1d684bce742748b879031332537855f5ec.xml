  <chunk id="fa93cc1d684bce742748b879031332537855f5ec.c0" text="Clinical Study The Effect of Postoperative Corticosteroid Administration on Free Vascularized Fibular Grafting for Treating Osteonecrosis of the Femoral Head">
    <entity charOffset="43-57" id="fa93cc1d684bce742748b879031332537855f5ec.c0.e0" ontology_id="CHEBI_50858" text="Corticosteroid" type="chemical"/>
    <entity charOffset="124-137" id="fa93cc1d684bce742748b879031332537855f5ec.c0.e1" ontology_id="DOID_10159" text="Osteonecrosis" type="disease"/>
    <pair e1="fa93cc1d684bce742748b879031332537855f5ec.c0.e0" e2="fa93cc1d684bce742748b879031332537855f5ec.c0.e1" id="fa93cc1d684bce742748b879031332537855f5ec.c0.p0" relation="true"/>
