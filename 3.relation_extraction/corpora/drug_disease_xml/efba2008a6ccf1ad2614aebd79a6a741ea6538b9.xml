  <chunk id="efba2008a6ccf1ad2614aebd79a6a741ea6538b9.c0" text="Immunomodulatory Activity and Protective Effects of Polysaccharide from Eupatorium adenophorum Leaf Extract on Highly Pathogenic H5N1 Influenza Infection">
    <entity charOffset="52-66" id="efba2008a6ccf1ad2614aebd79a6a741ea6538b9.c0.e0" ontology_id="CHEBI_18154" text="Polysaccharide" type="chemical"/>
