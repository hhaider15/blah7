  <chunk id="5edd56f96660760a5f8ae69efd7d9e976a00c4ee.c0" text="Immunogenicity of Virus Like Particle Forming Baculoviral DNA Vaccine against Pandemic Influenza H1N1">
    <entity charOffset="58-61" id="5edd56f96660760a5f8ae69efd7d9e976a00c4ee.c0.e0" ontology_id="CHEBI_16991" text="DNA" type="chemical"/>
