  <chunk id="9153d6b8fb3af63f68f15f52198cdbc308c97333.c0" text="IFITM3 Clusters on Virus Containing Endosomes and Lysosomes Early in the Influenza A Infection of Human Airway Epithelial Cells">
    <entity charOffset="7-15" id="9153d6b8fb3af63f68f15f52198cdbc308c97333.c0.e0" ontology_id="CHEBI_33731" text="Clusters" type="chemical"/>
