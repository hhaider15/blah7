  <chunk id="9be8c80283a7fed59057d7788eb2850708f45450.c0" text="curcumin to promote the Synthesis of Silver nps and their Self- Assembly with a thermoresponsive polymer in core-Shell nanohybrids">
    <entity charOffset="0-8" id="9be8c80283a7fed59057d7788eb2850708f45450.c0.e0" ontology_id="CHEBI_3962" text="curcumin" type="chemical"/>
    <entity charOffset="37-43" id="9be8c80283a7fed59057d7788eb2850708f45450.c0.e1" ontology_id="CHEBI_30512" text="Silver" type="chemical"/>
    <entity charOffset="97-104" id="9be8c80283a7fed59057d7788eb2850708f45450.c0.e2" ontology_id="CHEBI_33839" text="polymer" type="chemical"/>
