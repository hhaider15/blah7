  <chunk id="0304ed96b29c38014a17bacefcc294a02c04d0a0.c0" text="Statistical Evaluation of HTS Assays for Enzymatic Hydrolysis of β-Keto Esters">
    <entity charOffset="26-29" id="0304ed96b29c38014a17bacefcc294a02c04d0a0.c0.e0" ontology_id="DOID_0110698" text="HTS" type="disease"/>
    <entity charOffset="72-78" id="0304ed96b29c38014a17bacefcc294a02c04d0a0.c0.e1" ontology_id="CHEBI_35701" text="Esters" type="chemical"/>
