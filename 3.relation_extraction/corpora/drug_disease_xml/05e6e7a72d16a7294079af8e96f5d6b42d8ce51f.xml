  <chunk id="05e6e7a72d16a7294079af8e96f5d6b42d8ce51f.c0" text="Multiplex Reverse-Transcription Loop-Mediated Isothermal Amplification Coupled with Cascade Invasive Reaction and Nanoparticle Hybridization for Subtyping of Influenza A Virus">
    <entity charOffset="114-126" id="05e6e7a72d16a7294079af8e96f5d6b42d8ce51f.c0.e0" ontology_id="CHEBI_50803" text="Nanoparticle" type="chemical"/>
