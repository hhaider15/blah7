  <chunk id="aa6297e7fd55345b1ef69a2cbbb9629d155580a4.c0" text="Development and virucidal activity of a novel alcohol-based hand disinfectant supplemented with urea and citric acid">
    <entity charOffset="65-77" id="aa6297e7fd55345b1ef69a2cbbb9629d155580a4.c0.e0" ontology_id="CHEBI_48219" text="disinfectant" type="chemical"/>
