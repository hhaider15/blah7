  <chunk id="36885622f4e863ce69c521612093f1682fed1a5d.c0" text="BMC Medical Genomics The distinctive gastric fluid proteome in gastric cancer reveals a multi-biomarker diagnostic profile">
    <entity charOffset="0-3" id="36885622f4e863ce69c521612093f1682fed1a5d.c0.e0" ontology_id="CHEBI_3392" text="BMC" type="chemical"/>
