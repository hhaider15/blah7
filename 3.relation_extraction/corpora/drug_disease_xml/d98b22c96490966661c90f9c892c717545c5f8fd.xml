  <chunk id="d98b22c96490966661c90f9c892c717545c5f8fd.c0" text="Prohibitin plays a critical role in Enterovirus 71 neuropathogenesis Author summary">
    <entity charOffset="28-32" id="d98b22c96490966661c90f9c892c717545c5f8fd.c0.e0" ontology_id="CHEBI_50906" text="role" type="chemical"/>
