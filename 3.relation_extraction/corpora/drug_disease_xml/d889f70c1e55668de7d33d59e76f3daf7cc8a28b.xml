  <chunk id="d889f70c1e55668de7d33d59e76f3daf7cc8a28b.c0" text="Proteomics-Based Characterization of the Humoral Immune Response in Sporotrichosis: Toward Discovery of Potential Diagnostic and Vaccine Antigens">
    <entity charOffset="137-145" id="d889f70c1e55668de7d33d59e76f3daf7cc8a28b.c0.e0" ontology_id="CHEBI_59132" text="Antigens" type="chemical"/>
