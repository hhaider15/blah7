  <chunk id="6548b941beede8bb57cde83a928e4383b54697ad.c0" text="Povidone-iodine hand wash and hand rub products demonstrated excellent in vitro virucidal efficacy against Ebola virus and modified vaccinia virus Ankara, the new European test virus for enveloped viruses">
    <entity charOffset="0-15" id="6548b941beede8bb57cde83a928e4383b54697ad.c0.e0" ontology_id="CHEBI_8347" text="Povidone-iodine" type="chemical"/>
    <entity charOffset="132-140" id="6548b941beede8bb57cde83a928e4383b54697ad.c0.e1" ontology_id="DOID_3298" text="vaccinia" type="disease"/>
    <pair e1="6548b941beede8bb57cde83a928e4383b54697ad.c0.e0" e2="6548b941beede8bb57cde83a928e4383b54697ad.c0.e1" id="6548b941beede8bb57cde83a928e4383b54697ad.c0.p0" relation="true"/>
