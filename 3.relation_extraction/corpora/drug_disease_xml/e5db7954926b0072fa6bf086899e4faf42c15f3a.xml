  <chunk id="e5db7954926b0072fa6bf086899e4faf42c15f3a.c0" text="An M2e-based multiple antigenic peptide vaccine protects mice from lethal challenge with divergent H5N1 influenza viruses">
    <entity charOffset="32-39" id="e5db7954926b0072fa6bf086899e4faf42c15f3a.c0.e0" ontology_id="CHEBI_16670" text="peptide" type="chemical"/>
