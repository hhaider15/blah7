  <chunk id="215d86822474f970baa8ed894e23940bc4a85c79.c0" text="Molecular detection of infectious bronchitis and Newcastle disease viruses in broiler chickens with respiratory signs using Duplex RT-PCR">
    <entity charOffset="34-44" id="215d86822474f970baa8ed894e23940bc4a85c79.c0.e0" ontology_id="DOID_6132" text="bronchitis" type="disease"/>
    <entity charOffset="49-66" id="215d86822474f970baa8ed894e23940bc4a85c79.c0.e1" ontology_id="DOID_2929" text="Newcastle disease" type="disease"/>
    <entity charOffset="59-66" id="215d86822474f970baa8ed894e23940bc4a85c79.c0.e2" ontology_id="DOID_4" text="disease" type="disease"/>
