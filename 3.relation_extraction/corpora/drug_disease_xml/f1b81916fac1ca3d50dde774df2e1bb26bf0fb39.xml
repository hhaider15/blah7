  <chunk id="f1b81916fac1ca3d50dde774df2e1bb26bf0fb39.c0" text="The vacuolar-type ATPase inhibitor archazolid increases tumor cell adhesion to endothelial cells by accumulating extracellular collagen">
    <entity charOffset="18-34" id="f1b81916fac1ca3d50dde774df2e1bb26bf0fb39.c0.e0" ontology_id="CHEBI_78928" text="ATPase inhibitor" type="chemical"/>
    <entity charOffset="25-34" id="f1b81916fac1ca3d50dde774df2e1bb26bf0fb39.c0.e1" ontology_id="CHEBI_35222" text="inhibitor" type="chemical"/>
    <entity charOffset="127-135" id="f1b81916fac1ca3d50dde774df2e1bb26bf0fb39.c0.e2" ontology_id="CHEBI_3815" text="collagen" type="chemical"/>
