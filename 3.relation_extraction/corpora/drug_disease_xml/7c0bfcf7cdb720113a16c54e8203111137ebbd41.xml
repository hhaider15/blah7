  <chunk id="7c0bfcf7cdb720113a16c54e8203111137ebbd41.c0" text="Broad-spectrum antiviral activity of chebulagic acid and punicalagin against viruses that use glycosaminoglycans for entry">
    <entity charOffset="15-24" id="7c0bfcf7cdb720113a16c54e8203111137ebbd41.c0.e0" ontology_id="CHEBI_22587" text="antiviral" type="chemical"/>
