  <chunk id="91781004fcf2142034de62e1212be3d885139060.c0" text="Using GPS Technology to Quantify Human Mobility, Dynamic Contacts and Infectious Disease Dynamics in a Resource-Poor Urban Environment">
    <entity charOffset="6-9" id="91781004fcf2142034de62e1212be3d885139060.c0.e0" ontology_id="DOID_0111044" text="GPS" type="disease"/>
    <entity charOffset="70-88" id="91781004fcf2142034de62e1212be3d885139060.c0.e1" ontology_id="DOID_0050117" text="Infectious Disease" type="disease"/>
