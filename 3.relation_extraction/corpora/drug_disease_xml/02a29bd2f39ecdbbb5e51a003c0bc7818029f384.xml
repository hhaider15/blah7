  <chunk id="02a29bd2f39ecdbbb5e51a003c0bc7818029f384.c0" text="A Recombinant Influenza A/H1N1 Carrying A Short Immunogenic Peptide of MERS-CoV as Bivalent Vaccine in BALB/c Mice">
    <entity charOffset="14-23" id="02a29bd2f39ecdbbb5e51a003c0bc7818029f384.c0.e0" ontology_id="DOID_8469" text="Influenza" type="disease"/>
