  <chunk id="9f63ebfaab049c0968d72166761217b3aaa1fe00.c0" text="Open Access R440 Pneumothorax and mortality in the mechanically ventilated SARS patients: a prospective clinical study">
    <entity charOffset="5-11" id="9f63ebfaab049c0968d72166761217b3aaa1fe00.c0.e0" ontology_id="CHEBI_34922" text="Access" type="chemical"/>
    <entity charOffset="17-29" id="9f63ebfaab049c0968d72166761217b3aaa1fe00.c0.e1" ontology_id="DOID_1673" text="Pneumothorax" type="disease"/>
