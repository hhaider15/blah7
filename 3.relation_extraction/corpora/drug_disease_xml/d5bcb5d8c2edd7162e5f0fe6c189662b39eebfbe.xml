  <chunk id="d5bcb5d8c2edd7162e5f0fe6c189662b39eebfbe.c0" text="The Role of EGFR in Influenza Pathogenicity: Multiple Network-Based Approaches to Identify a Key Regulator of Non-lethal Infections">
    <entity charOffset="4-8" id="d5bcb5d8c2edd7162e5f0fe6c189662b39eebfbe.c0.e0" ontology_id="CHEBI_50906" text="Role" type="chemical"/>
