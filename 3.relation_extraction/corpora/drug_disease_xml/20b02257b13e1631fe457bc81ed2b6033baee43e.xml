  <chunk id="20b02257b13e1631fe457bc81ed2b6033baee43e.c0" text="BMC Infectious Diseases Lack of association between polymorphisms of MASP2 and susceptibility to SARS coronavirus infection">
    <entity charOffset="0-3" id="20b02257b13e1631fe457bc81ed2b6033baee43e.c0.e0" ontology_id="CHEBI_3392" text="BMC" type="chemical"/>
    <entity charOffset="97-101" id="20b02257b13e1631fe457bc81ed2b6033baee43e.c0.e1" ontology_id="DOID_2945" text="SARS" type="disease"/>
