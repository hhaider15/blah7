  <chunk id="15a5a243bb623a17ee4f0c18c4e3bea8c065ed11.c0" text="Transmissible Gastroenteritis Virus Infection Enhances SGLT1 and GLUT2 Expression to Increase Glucose Uptake">
    <entity charOffset="14-29" id="15a5a243bb623a17ee4f0c18c4e3bea8c065ed11.c0.e0" ontology_id="DOID_2326" text="Gastroenteritis" type="disease"/>
