  <chunk id="09de57e5401565a1e80361d32b09ce66b3a988c8.c0" text="Preliminary Identification of Potential Vaccine Targets for the COVID-19 Coronavirus (SARS-CoV-2) Based on SARS-CoV Immunological Studies">
    <entity charOffset="64-72" id="09de57e5401565a1e80361d32b09ce66b3a988c8.c0.e0" ontology_id="DOID_0080600" text="COVID-19" type="disease"/>
