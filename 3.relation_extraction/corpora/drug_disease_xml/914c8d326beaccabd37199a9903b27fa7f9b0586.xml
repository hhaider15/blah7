  <chunk id="914c8d326beaccabd37199a9903b27fa7f9b0586.c0" text="Pseudoknots: RNA Structures with Diverse Functions">
    <entity charOffset="13-16" id="914c8d326beaccabd37199a9903b27fa7f9b0586.c0.e0" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
