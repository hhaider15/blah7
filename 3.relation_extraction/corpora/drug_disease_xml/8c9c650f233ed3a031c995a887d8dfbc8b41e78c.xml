  <chunk id="8c9c650f233ed3a031c995a887d8dfbc8b41e78c.c0" text="Feline Infectious Peritonitis Virus Nsp5 Inhibits Type I Interferon Production by Cleaving NEMO at Multiple Sites">
    <entity charOffset="18-29" id="8c9c650f233ed3a031c995a887d8dfbc8b41e78c.c0.e0" ontology_id="DOID_8283" text="Peritonitis" type="disease"/>
    <entity charOffset="57-67" id="8c9c650f233ed3a031c995a887d8dfbc8b41e78c.c0.e1" ontology_id="CHEBI_52999" text="Interferon" type="chemical"/>
    <pair e1="8c9c650f233ed3a031c995a887d8dfbc8b41e78c.c0.e0" e2="8c9c650f233ed3a031c995a887d8dfbc8b41e78c.c0.e1" id="8c9c650f233ed3a031c995a887d8dfbc8b41e78c.c0.p0" relation="true"/>
