  <chunk id="e58bb3ad5e35a2895353e82c3690e2dd33a8d86b.c0" text="SPI-1 is a missing host-range factor required for replication of the attenuated modified vaccinia Ankara (MVA) vaccine vector in human cells">
    <entity charOffset="89-97" id="e58bb3ad5e35a2895353e82c3690e2dd33a8d86b.c0.e0" ontology_id="DOID_3298" text="vaccinia" type="disease"/>
