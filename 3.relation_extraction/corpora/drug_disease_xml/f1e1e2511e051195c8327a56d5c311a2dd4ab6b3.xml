  <chunk id="f1e1e2511e051195c8327a56d5c311a2dd4ab6b3.c0" text="viruses Gemcitabine and Nucleos(t)ide Synthesis Inhibitors Are Broad-Spectrum Antiviral Drugs that Activate Innate Immunity">
    <entity charOffset="8-19" id="f1e1e2511e051195c8327a56d5c311a2dd4ab6b3.c0.e0" ontology_id="CHEBI_175901" text="Gemcitabine" type="chemical"/>
