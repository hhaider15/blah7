  <chunk id="369914e87f682579eb3a5efeb43dc0184a88b5d6.c0" text="Combination Kinase Inhibitor Treatment Suppresses Rift Valley Fever Virus Replication">
    <entity charOffset="19-28" id="369914e87f682579eb3a5efeb43dc0184a88b5d6.c0.e0" ontology_id="CHEBI_35222" text="Inhibitor" type="chemical"/>
    <entity charOffset="50-67" id="369914e87f682579eb3a5efeb43dc0184a88b5d6.c0.e1" ontology_id="DOID_1328" text="Rift Valley Fever" type="disease"/>
    <pair e1="369914e87f682579eb3a5efeb43dc0184a88b5d6.c0.e0" e2="369914e87f682579eb3a5efeb43dc0184a88b5d6.c0.e1" id="369914e87f682579eb3a5efeb43dc0184a88b5d6.c0.p0" relation="true"/>
