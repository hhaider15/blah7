  <chunk id="b11cf879da2036d92fbdbbca07fd8cb7f0416e5f.c0" text="A Rationally Designed TNF-α Epitope- Scaffold Immunogen Induces Sustained Antibody Response and Alleviates Collagen- Induced Arthritis in Mice">
    <entity charOffset="46-55" id="b11cf879da2036d92fbdbbca07fd8cb7f0416e5f.c0.e0" ontology_id="CHEBI_60816" text="Immunogen" type="chemical"/>
