  <chunk id="bcb10d6f9d4c95cfcd762fe6297dcb46bc15ec3f.c0" text="Novel Inhibitor Design for Hemagglutinin against H1N1 Influenza Virus by Core Hopping Method">
    <entity charOffset="6-15" id="bcb10d6f9d4c95cfcd762fe6297dcb46bc15ec3f.c0.e0" ontology_id="CHEBI_35222" text="Inhibitor" type="chemical"/>
