  <chunk id="8e0621d9c791cda1b772290d37ab245bf7023514.c0" text="viruses Communication Arbidol (Umifenovir): A Broad-Spectrum Antiviral Drug That Inhibits Medically Important Arthropod-Borne Flaviviruses">
    <entity charOffset="22-29" id="8e0621d9c791cda1b772290d37ab245bf7023514.c0.e0" ontology_id="CHEBI_134730" text="Arbidol" type="chemical"/>
