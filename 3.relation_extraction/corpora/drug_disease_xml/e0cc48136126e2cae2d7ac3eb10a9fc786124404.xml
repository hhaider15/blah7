  <chunk id="e0cc48136126e2cae2d7ac3eb10a9fc786124404.c0" text="Development of the full-length cDNA clones of two porcine epidemic diarrhea disease virus isolates with different virulence">
    <entity charOffset="67-75" id="e0cc48136126e2cae2d7ac3eb10a9fc786124404.c0.e0" ontology_id="DOID_13250" text="diarrhea" type="disease"/>
    <entity charOffset="76-83" id="e0cc48136126e2cae2d7ac3eb10a9fc786124404.c0.e1" ontology_id="DOID_4" text="disease" type="disease"/>
