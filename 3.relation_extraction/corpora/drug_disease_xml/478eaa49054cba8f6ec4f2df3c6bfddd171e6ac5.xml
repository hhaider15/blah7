    <entity charOffset="481-508" id="478eaa49054cba8f6ec4f2df3c6bfddd171e6ac5.c3.e6" ontology_id="CHEBI_78782" text="pyrrolidine dithiocarbamate" type="chemical"/>
    <entity charOffset="525-532" id="478eaa49054cba8f6ec4f2df3c6bfddd171e6ac5.c3.e7" ontology_id="CHEBI_33229" text="vitamin" type="chemical"/>
    <entity charOffset="525-534" id="478eaa49054cba8f6ec4f2df3c6bfddd171e6ac5.c3.e8" ontology_id="CHEBI_18145" text="vitamin E" type="chemical"/>
    <entity charOffset="525-534" id="478eaa49054cba8f6ec4f2df3c6bfddd171e6ac5.c3.e9" ontology_id="CHEBI_33234" text="vitamin E" type="chemical"/>
  </chunk>
  <chunk id="478eaa49054cba8f6ec4f2df3c6bfddd171e6ac5.c4" text="We concluded that the inhibition of PEDV production by Q7R is not simply due to a general action as an antioxidants and is highly specific, as several other antioxidants (NAC, PDTC, trolox) are inactive against PEDV infection.">
    <entity charOffset="90-96" id="478eaa49054cba8f6ec4f2df3c6bfddd171e6ac5.c4.e0" ontology_id="CHEBI_5133" text="action" type="chemical"/>
