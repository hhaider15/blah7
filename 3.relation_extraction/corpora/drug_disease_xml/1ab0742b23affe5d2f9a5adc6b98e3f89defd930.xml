  <chunk id="1ab0742b23affe5d2f9a5adc6b98e3f89defd930.c0" text="Usefulness of Cellular Analysis of Bronchoalveolar Lavage Fluid for Predicting the Etiology of Pneumonia in Critically Ill Patients">
    <entity charOffset="95-104" id="1ab0742b23affe5d2f9a5adc6b98e3f89defd930.c0.e0" ontology_id="DOID_552" text="Pneumonia" type="disease"/>
