  <chunk id="1560867701aecc60f73407501a5c5c85e53c8522.c0" text="MAVS-Mediated Apoptosis and Its Inhibition by Viral Proteins">
    <entity charOffset="52-60" id="1560867701aecc60f73407501a5c5c85e53c8522.c0.e0" ontology_id="CHEBI_36080" text="Proteins" type="chemical"/>
