  <chunk id="1e8ec294fcc4507fcc89872f84adcf4633bb10e6.c0" text="Evaluation of Commercial Diagnostic Assays for the Specific Detection of Avian Influenza A (H7N9) Virus RNA Using a Quality-Control Panel and Clinical Specimens in China">
    <entity charOffset="73-88" id="1e8ec294fcc4507fcc89872f84adcf4633bb10e6.c0.e0" ontology_id="DOID_4492" text="Avian Influenza" type="disease"/>
