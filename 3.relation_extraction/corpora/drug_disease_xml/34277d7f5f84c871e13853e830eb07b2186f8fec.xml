  <chunk id="34277d7f5f84c871e13853e830eb07b2186f8fec.c0" text="Delta inulin-based adjuvants promote the generation of polyfunctional CD4 + T cell responses and protection against Mycobacterium tuberculosis infection">
    <entity charOffset="19-28" id="34277d7f5f84c871e13853e830eb07b2186f8fec.c0.e0" ontology_id="CHEBI_60809" text="adjuvants" type="chemical"/>
