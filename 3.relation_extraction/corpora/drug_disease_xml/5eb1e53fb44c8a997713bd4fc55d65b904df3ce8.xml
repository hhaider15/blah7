  <chunk id="5eb1e53fb44c8a997713bd4fc55d65b904df3ce8.c0" text="Silver Sucrose Octasulfate (IASOS TM ) as a Valid Active Ingredient into a Novel Vaginal Gel against Human Vaginal Pathogens: In Vitro Antimicrobial Activity Assessment">
    <entity charOffset="0-6" id="5eb1e53fb44c8a997713bd4fc55d65b904df3ce8.c0.e0" ontology_id="CHEBI_30512" text="Silver" type="chemical"/>
