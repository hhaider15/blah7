  <chunk id="ac8d2e900ee0c6d713d153782a530ddee6d606aa.c0" text="Influenza A virus PB1-F2 protein prolongs viral shedding in chickens lengthening the transmission window">
    <entity charOffset="0-9" id="ac8d2e900ee0c6d713d153782a530ddee6d606aa.c0.e0" ontology_id="DOID_8469" text="Influenza" type="disease"/>
    <entity charOffset="25-32" id="ac8d2e900ee0c6d713d153782a530ddee6d606aa.c0.e1" ontology_id="CHEBI_16541" text="protein" type="chemical"/>
    <pair e1="ac8d2e900ee0c6d713d153782a530ddee6d606aa.c0.e0" e2="ac8d2e900ee0c6d713d153782a530ddee6d606aa.c0.e1" id="ac8d2e900ee0c6d713d153782a530ddee6d606aa.c0.p0" relation="true"/>
