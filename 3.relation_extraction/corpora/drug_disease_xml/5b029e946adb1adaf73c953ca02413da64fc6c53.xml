  <chunk id="5b029e946adb1adaf73c953ca02413da64fc6c53.c0" text="Use of a Small Peptide Fragment as an Inhibitor of Insulin Fibrillation Process: A Study by High and Low Resolution Spectroscopy">
    <entity charOffset="15-22" id="5b029e946adb1adaf73c953ca02413da64fc6c53.c0.e0" ontology_id="CHEBI_16670" text="Peptide" type="chemical"/>
