  <chunk id="ed526e1eb5a7607af2803906e2f6d4e5bb2dff09.c0" text="Potential Broad Spectrum Inhibitors of the Coronavirus 3CL pro : A Virtual Screening and Structure-Based Drug Design Study">
    <entity charOffset="25-35" id="ed526e1eb5a7607af2803906e2f6d4e5bb2dff09.c0.e0" ontology_id="CHEBI_35222" text="Inhibitors" type="chemical"/>
