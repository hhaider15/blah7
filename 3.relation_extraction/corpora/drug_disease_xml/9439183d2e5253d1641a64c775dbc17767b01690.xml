  <chunk id="9439183d2e5253d1641a64c775dbc17767b01690.c0" text="Altered Lipid Metabolism in Recovered SARS Patients Twelve Years after Infection OPEN">
    <entity charOffset="8-13" id="9439183d2e5253d1641a64c775dbc17767b01690.c0.e0" ontology_id="CHEBI_18059" text="Lipid" type="chemical"/>
