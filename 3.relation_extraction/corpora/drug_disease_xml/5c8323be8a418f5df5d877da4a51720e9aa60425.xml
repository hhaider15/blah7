  <chunk id="5c8323be8a418f5df5d877da4a51720e9aa60425.c0" text="AGING NEUROSCIENCE REVIEW ARTICLE Herpes simplex virus type 1 and Alzheimer's disease: increasing evidence for a major role of the virus">
    <entity charOffset="34-48" id="5c8323be8a418f5df5d877da4a51720e9aa60425.c0.e0" ontology_id="DOID_8566" text="Herpes simplex" type="disease"/>
