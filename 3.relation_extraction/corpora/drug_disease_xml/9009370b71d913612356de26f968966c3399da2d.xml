  <chunk id="9009370b71d913612356de26f968966c3399da2d.c0" text="Proteomic analysis of purified Newcastle disease virus particles">
    <entity charOffset="31-48" id="9009370b71d913612356de26f968966c3399da2d.c0.e0" ontology_id="DOID_2929" text="Newcastle disease" type="disease"/>
    <entity charOffset="41-48" id="9009370b71d913612356de26f968966c3399da2d.c0.e1" ontology_id="DOID_4" text="disease" type="disease"/>
