  <chunk id="8602709a812146a012e16df450b991f6343d2f46.c0" text="Suppression of Virulent Porcine Epidemic Diarrhea Virus Proliferation by the PI3K/Akt/ GSK-3α/β Pathway">
    <entity charOffset="41-49" id="8602709a812146a012e16df450b991f6343d2f46.c0.e0" ontology_id="DOID_13250" text="Diarrhea" type="disease"/>
    <entity charOffset="96-103" id="8602709a812146a012e16df450b991f6343d2f46.c0.e1" ontology_id="CHEBI_34922" text="Pathway" type="chemical"/>
    <pair e1="8602709a812146a012e16df450b991f6343d2f46.c0.e0" e2="8602709a812146a012e16df450b991f6343d2f46.c0.e1" id="8602709a812146a012e16df450b991f6343d2f46.c0.p0" relation="true"/>
