  <chunk id="7ec733f3684d6294483b03df183878ad777a78d9.c0" text="Future Path Toward TB Vaccine Development: Boosting BCG or Re-educating by a New Subunit Vaccine Strategy for Boosting BCG Vaccine">
    <entity charOffset="52-55" id="7ec733f3684d6294483b03df183878ad777a78d9.c0.e0" ontology_id="CHEBI_41001" text="BCG" type="chemical"/>
    <entity charOffset="119-122" id="7ec733f3684d6294483b03df183878ad777a78d9.c0.e1" ontology_id="CHEBI_41001" text="BCG" type="chemical"/>
