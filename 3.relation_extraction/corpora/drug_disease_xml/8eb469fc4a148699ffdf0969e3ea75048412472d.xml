  <chunk id="8eb469fc4a148699ffdf0969e3ea75048412472d.c0" text="The YXXΦ motif within the severe acute respiratory syndrome coronavirus (SARS-CoV) 3a protein is crucial for its intracellular transport">
    <entity charOffset="26-59" id="8eb469fc4a148699ffdf0969e3ea75048412472d.c0.e0" ontology_id="DOID_2945" text="severe acute respiratory syndrome" type="disease"/>
