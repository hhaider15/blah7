  <chunk id="b1be7d31c3101fa5b0c803ee2d354eb7d3943526.c0" text="Gemcitabine, a broad-spectrum antiviral drug, suppresses enterovirus infections through innate immunity induced by the inhibition of pyrimidine biosynthesis and nucleotide depletion">
    <entity charOffset="30-39" id="b1be7d31c3101fa5b0c803ee2d354eb7d3943526.c0.e0" ontology_id="CHEBI_22587" text="antiviral" type="chemical"/>
