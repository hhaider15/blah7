  <chunk id="a7fa116862ab1522f6ccbe6c261e83722fca138b.c0" text="Role of fomites in SARS transmission during the largest hospital outbreak in Hong Kong">
    <entity charOffset="0-4" id="a7fa116862ab1522f6ccbe6c261e83722fca138b.c0.e0" ontology_id="CHEBI_50906" text="Role" type="chemical"/>
    <entity charOffset="19-23" id="a7fa116862ab1522f6ccbe6c261e83722fca138b.c0.e1" ontology_id="DOID_2945" text="SARS" type="disease"/>
