  <chunk id="a7655a74468e485dfd7f45390c5be4c4aa9361a8.c0" text="Comparative Epidemiology of Human Infections with Middle East Respiratory Syndrome and Severe Acute Respiratory Syndrome Coronaviruses among Healthcare Personnel">
    <entity charOffset="50-82" id="a7655a74468e485dfd7f45390c5be4c4aa9361a8.c0.e0" ontology_id="DOID_0080642" text="Middle East Respiratory Syndrome" type="disease"/>
