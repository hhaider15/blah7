  <chunk id="7379b8c28321b7f9c78d2b4d35109a87b35cf883.c0" text="Interaction of the Coronavirus Infectious Bronchitis Virus Membrane Protein with b-Actin and Its Implication in Virion Assembly and Budding">
    <entity charOffset="42-52" id="7379b8c28321b7f9c78d2b4d35109a87b35cf883.c0.e0" ontology_id="DOID_6132" text="Bronchitis" type="disease"/>
