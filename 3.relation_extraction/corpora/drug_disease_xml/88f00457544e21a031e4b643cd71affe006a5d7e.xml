  <chunk id="88f00457544e21a031e4b643cd71affe006a5d7e.c0" text="Clinical outcomes of patients treated with intravenous zanamivir for severe influenza A(H1N1)pdm09 infection: a case report series">
    <entity charOffset="55-64" id="88f00457544e21a031e4b643cd71affe006a5d7e.c0.e0" ontology_id="CHEBI_50663" text="zanamivir" type="chemical"/>
    <entity charOffset="76-85" id="88f00457544e21a031e4b643cd71affe006a5d7e.c0.e1" ontology_id="DOID_8469" text="influenza" type="disease"/>
    <pair e1="88f00457544e21a031e4b643cd71affe006a5d7e.c0.e0" e2="88f00457544e21a031e4b643cd71affe006a5d7e.c0.e1" id="88f00457544e21a031e4b643cd71affe006a5d7e.c0.p0" relation="true"/>
