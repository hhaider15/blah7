  <chunk id="c4f28354c6015236a080fc0d67b0fc9d193c70d6.c0" text="Circular RNA CircEZH2 Suppresses Transmissible Gastroenteritis Coronavirus-induced Opening of Mitochondrial Permeability Transition Pore via Targeting MiR-22 in IPEC-J2">
    <entity charOffset="9-12" id="c4f28354c6015236a080fc0d67b0fc9d193c70d6.c0.e0" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
    <entity charOffset="47-62" id="c4f28354c6015236a080fc0d67b0fc9d193c70d6.c0.e1" ontology_id="DOID_2326" text="Gastroenteritis" type="disease"/>
    <pair e1="c4f28354c6015236a080fc0d67b0fc9d193c70d6.c0.e0" e2="c4f28354c6015236a080fc0d67b0fc9d193c70d6.c0.e1" id="c4f28354c6015236a080fc0d67b0fc9d193c70d6.c0.p0" relation="true"/>
