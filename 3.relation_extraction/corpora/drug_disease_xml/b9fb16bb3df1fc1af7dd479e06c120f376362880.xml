  <chunk id="b9fb16bb3df1fc1af7dd479e06c120f376362880.c0" text="High levels of unreported intraspecific diversity among RNA viruses in faeces of neonatal piglets with diarrhoea">
    <entity charOffset="56-59" id="b9fb16bb3df1fc1af7dd479e06c120f376362880.c0.e0" ontology_id="CHEBI_33697" text="RNA" type="chemical"/>
    <entity charOffset="103-112" id="b9fb16bb3df1fc1af7dd479e06c120f376362880.c0.e1" ontology_id="DOID_13250" text="diarrhoea" type="disease"/>
    <pair e1="b9fb16bb3df1fc1af7dd479e06c120f376362880.c0.e0" e2="b9fb16bb3df1fc1af7dd479e06c120f376362880.c0.e1" id="b9fb16bb3df1fc1af7dd479e06c120f376362880.c0.p0" relation="true"/>
