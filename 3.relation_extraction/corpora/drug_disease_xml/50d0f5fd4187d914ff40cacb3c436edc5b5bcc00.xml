  <chunk id="50d0f5fd4187d914ff40cacb3c436edc5b5bcc00.c0" text="Infection-mediated asthma: etiology, mechanisms and treatment options, with focus on Chlamydia pneumoniae and macrolides">
    <entity charOffset="85-94" id="50d0f5fd4187d914ff40cacb3c436edc5b5bcc00.c0.e0" ontology_id="DOID_11263" text="Chlamydia" type="disease"/>
