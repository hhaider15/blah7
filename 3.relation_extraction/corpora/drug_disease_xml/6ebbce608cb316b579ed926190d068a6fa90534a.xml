  <chunk id="6ebbce608cb316b579ed926190d068a6fa90534a.c0" text="R E V I E W Open Access 'The Microbiome and the Pathophysiology of Asthma'">
    <entity charOffset="17-23" id="6ebbce608cb316b579ed926190d068a6fa90534a.c0.e0" ontology_id="CHEBI_34922" text="Access" type="chemical"/>
