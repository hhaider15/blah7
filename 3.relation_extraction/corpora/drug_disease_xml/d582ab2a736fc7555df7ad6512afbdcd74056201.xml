  <chunk id="d582ab2a736fc7555df7ad6512afbdcd74056201.c0" text="Increased ATP generation in the host cell is required for efficient vaccinia virus production">
    <entity charOffset="10-13" id="d582ab2a736fc7555df7ad6512afbdcd74056201.c0.e0" ontology_id="CHEBI_15422" text="ATP" type="chemical"/>
