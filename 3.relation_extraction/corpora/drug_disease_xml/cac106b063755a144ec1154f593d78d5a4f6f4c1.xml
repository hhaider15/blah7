  <chunk id="cac106b063755a144ec1154f593d78d5a4f6f4c1.c0" text="Porcine transmissible gastroenteritis virus nonstructural protein 2 contributes to inflammation via NF-κB activation">
    <entity charOffset="22-37" id="cac106b063755a144ec1154f593d78d5a4f6f4c1.c0.e0" ontology_id="DOID_2326" text="gastroenteritis" type="disease"/>
