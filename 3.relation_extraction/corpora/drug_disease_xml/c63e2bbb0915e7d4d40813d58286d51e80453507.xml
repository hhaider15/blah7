  <chunk id="c63e2bbb0915e7d4d40813d58286d51e80453507.c0" text="Efficient generation of influenza virus with a mouse RNA polymerase I-driven all-in-one plasmid">
    <entity charOffset="24-33" id="c63e2bbb0915e7d4d40813d58286d51e80453507.c0.e0" ontology_id="DOID_8469" text="influenza" type="disease"/>
