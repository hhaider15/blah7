    <entity charOffset="382-398" id="c7d8100f9f9f799f495a0458c8f23dcc7bcec3ec.c2.e3" ontology_id="DOID_6132" text="acute bronchitis" type="disease"/>
    <entity charOffset="388-398" id="c7d8100f9f9f799f495a0458c8f23dcc7bcec3ec.c2.e4" ontology_id="DOID_6132" text="bronchitis" type="disease"/>
  </chunk>
  <chunk id="c7d8100f9f9f799f495a0458c8f23dcc7bcec3ec.c3" text="The effects of Cineole in the treatment of acute bronchitis were clearly measurable and could be proven after a treatment period of merely 4 days. This study corroborates the fact that Cineole actively and significantly reduces cough frequency after four days. Therefore it has been shown to have a great socioeconomic impact.">
    <entity charOffset="15-22" id="c7d8100f9f9f799f495a0458c8f23dcc7bcec3ec.c3.e0" ontology_id="CHEBI_23243" text="Cineole" type="chemical"/>
