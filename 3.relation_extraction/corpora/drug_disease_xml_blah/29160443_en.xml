  <chunk id="29160443_en.c0" text="Body fat redistribution and changes in lipid and glucose metabolism in people living with HIV/AIDS.">
    <entity charOffset="39-44" id="29160443_en.c0.e0" ontology_id="CHEBI_18059" text="lipid" type="chemical"/>
    <entity charOffset="49-56" id="29160443_en.c0.e1" ontology_id="CHEBI_17234" text="glucose" type="chemical"/>
