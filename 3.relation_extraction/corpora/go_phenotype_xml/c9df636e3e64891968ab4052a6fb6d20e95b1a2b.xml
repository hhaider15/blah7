  <chunk id="c9df636e3e64891968ab4052a6fb6d20e95b1a2b.c0" text="Cellular hnRNP A1 Interacts with Nucleocapsid Protein of Porcine Epidemic Diarrhea Virus and Impairs Viral Replication">
    <entity charOffset="33-45" id="c9df636e3e64891968ab4052a6fb6d20e95b1a2b.c0.e0" ontology_id="GO_0019013" text="Nucleocapsid" type="gene_function"/>
