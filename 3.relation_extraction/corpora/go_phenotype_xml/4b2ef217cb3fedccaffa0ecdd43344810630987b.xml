  <chunk id="4b2ef217cb3fedccaffa0ecdd43344810630987b.c0" text="Identification of human leukemia antigen A*0201-restricted epitopes derived from epidermal growth factor pathway substrate number 8">
    <entity charOffset="24-32" id="4b2ef217cb3fedccaffa0ecdd43344810630987b.c0.e0" ontology_id="HP_0001909" text="leukemia" type="phenotype"/>
    <entity charOffset="91-97" id="4b2ef217cb3fedccaffa0ecdd43344810630987b.c0.e1" ontology_id="GO_0040007" text="growth" type="gene_function"/>
