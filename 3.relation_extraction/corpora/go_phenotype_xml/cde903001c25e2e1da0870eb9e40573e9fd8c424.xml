  <chunk id="cde903001c25e2e1da0870eb9e40573e9fd8c424.c0" text="Disorders of the Nervous System Aging Does Not Affect Axon Initial Segment Structure and Somatic Localization of Tau Protein in Hippocampal Neurons of Fischer 344 Rats">
    <entity charOffset="32-37" id="cde903001c25e2e1da0870eb9e40573e9fd8c424.c0.e0" ontology_id="GO_0007568" text="Aging" type="gene_function"/>
