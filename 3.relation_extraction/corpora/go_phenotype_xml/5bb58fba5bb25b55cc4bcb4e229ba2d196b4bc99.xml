  <chunk id="5bb58fba5bb25b55cc4bcb4e229ba2d196b4bc99.c0" text="Global analysis of polysome-associated mRNA in vesicular stomatitis virus infected cells">
    <entity charOffset="57-67" id="5bb58fba5bb25b55cc4bcb4e229ba2d196b4bc99.c0.e0" ontology_id="HP_0010280" text="stomatitis" type="phenotype"/>
