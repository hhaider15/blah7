  <chunk id="11bf75e973011d86b1039b707ed086b98deb5642.c0" text="Human Monoclonal Antibodies as Adjuvant Treatment of Chronic Hepatitis B Virus Infection">
    <entity charOffset="53-60" id="11bf75e973011d86b1039b707ed086b98deb5642.c0.e0" ontology_id="HP_0011010" text="Chronic" type="phenotype"/>
