  <chunk id="8863943c0c9153a3677761f16e704854c65c3783.c0" text="EGFR as a Negative Regulatory Protein Adjusts the Activity and Mobility of NHE3 in the Cell Membrane of IPEC-J2 Cells With TGEV Infection">
    <entity charOffset="0-4" id="8863943c0c9153a3677761f16e704854c65c3783.c0.e0" ontology_id="GO_0005006" text="EGFR" type="gene_function"/>
    <entity charOffset="87-100" id="8863943c0c9153a3677761f16e704854c65c3783.c0.e1" ontology_id="GO_0005886" text="Cell Membrane" type="gene_function"/>
    <entity charOffset="92-100" id="8863943c0c9153a3677761f16e704854c65c3783.c0.e2" ontology_id="GO_0016020" text="Membrane" type="gene_function"/>
