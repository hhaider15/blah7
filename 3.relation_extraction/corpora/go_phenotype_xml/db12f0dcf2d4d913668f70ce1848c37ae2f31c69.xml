  <chunk id="db12f0dcf2d4d913668f70ce1848c37ae2f31c69.c0" text="Cerebrospinal fluid chemokine patterns in children with enterovirus 71-related encephalitis">
    <entity charOffset="79-91" id="db12f0dcf2d4d913668f70ce1848c37ae2f31c69.c0.e0" ontology_id="HP_0002383" text="encephalitis" type="phenotype"/>
