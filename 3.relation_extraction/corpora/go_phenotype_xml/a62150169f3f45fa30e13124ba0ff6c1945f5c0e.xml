  <chunk id="a62150169f3f45fa30e13124ba0ff6c1945f5c0e.c0" text="Host phosphatidic acid phosphatase lipin1 is rate limiting for functional hepatitis C virus replicase complex formation Author summary">
    <entity charOffset="23-34" id="a62150169f3f45fa30e13124ba0ff6c1945f5c0e.c0.e0" ontology_id="GO_0016791" text="phosphatase" type="gene_function"/>
