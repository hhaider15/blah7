  <chunk id="7e0925a800d4d09380ac44584b5be257d83531d3.c0" text="Molecular Diagnosis of Pneumonia Using Multiplex Real-Time PCR Assay RespiFinder ® SMART 22 FAST in a Group of Moroccan Infants">
    <entity charOffset="23-32" id="7e0925a800d4d09380ac44584b5be257d83531d3.c0.e0" ontology_id="HP_0002090" text="Pneumonia" type="phenotype"/>
    <entity charOffset="92-96" id="7e0925a800d4d09380ac44584b5be257d83531d3.c0.e1" ontology_id="GO_0033867" text="FAST" type="gene_function"/>
