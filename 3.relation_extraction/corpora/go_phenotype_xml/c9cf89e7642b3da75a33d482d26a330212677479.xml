  <chunk id="c9cf89e7642b3da75a33d482d26a330212677479.c4" text="Autophagy and Ubiquitin System: A Brief Overview">
    <entity charOffset="0-9" id="c9cf89e7642b3da75a33d482d26a330212677479.c4.e0" ontology_id="GO_0006914" text="Autophagy" type="gene_function"/>
    <entity charOffset="14-23" id="c9cf89e7642b3da75a33d482d26a330212677479.c4.e1" ontology_id="GO_0031386" text="Ubiquitin" type="gene_function"/>
