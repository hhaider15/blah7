  <chunk id="7f67490473a643ff59b8ad115149846bc36825ca.c0" text="Host genetic background influences diverse neurological responses to viral infection in mice OPEN">
    <entity charOffset="69-84" id="7f67490473a643ff59b8ad115149846bc36825ca.c0.e0" ontology_id="GO_0016032" text="viral infection" type="gene_function"/>
