  <chunk id="959b3431d24276e1b5d1bcfa7885979a4310bd98.c0" text="Type III Interferon-Mediated Signaling Is Critical for Controlling Live Attenuated Yellow Fever Virus Infection In Vivo">
    <entity charOffset="29-38" id="959b3431d24276e1b5d1bcfa7885979a4310bd98.c0.e0" ontology_id="GO_0023052" text="Signaling" type="gene_function"/>
    <entity charOffset="90-95" id="959b3431d24276e1b5d1bcfa7885979a4310bd98.c0.e1" ontology_id="HP_0001945" text="Fever" type="phenotype"/>
    <pair e1="959b3431d24276e1b5d1bcfa7885979a4310bd98.c0.e0" e2="959b3431d24276e1b5d1bcfa7885979a4310bd98.c0.e1" id="959b3431d24276e1b5d1bcfa7885979a4310bd98.c0.p0" relation="true"/>
