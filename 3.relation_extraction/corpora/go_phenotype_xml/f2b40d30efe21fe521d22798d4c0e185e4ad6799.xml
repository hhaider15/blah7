  <chunk id="f2b40d30efe21fe521d22798d4c0e185e4ad6799.c0" text="PCR based bronchoscopic detection of common respiratory pathogens in chronic cough: a case control study">
    <entity charOffset="69-76" id="f2b40d30efe21fe521d22798d4c0e185e4ad6799.c0.e0" ontology_id="HP_0011010" text="chronic" type="phenotype"/>
