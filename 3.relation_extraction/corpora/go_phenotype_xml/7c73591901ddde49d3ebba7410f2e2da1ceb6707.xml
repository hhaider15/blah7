  <chunk id="7c73591901ddde49d3ebba7410f2e2da1ceb6707.c0" text="Towards Systematic Discovery of Signaling Networks in Budding Yeast Filamentous Growth Stress Response Using Interventional Phosphorylation Data">
    <entity charOffset="32-41" id="7c73591901ddde49d3ebba7410f2e2da1ceb6707.c0.e0" ontology_id="GO_0023052" text="Signaling" type="gene_function"/>
