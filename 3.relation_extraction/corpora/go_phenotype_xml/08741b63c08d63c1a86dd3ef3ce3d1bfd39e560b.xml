  <chunk id="08741b63c08d63c1a86dd3ef3ce3d1bfd39e560b.c0" text="Dissection and integration of the autophagy signaling network initiated by bluetongue virus infection: crucial candidates ERK1/2, Akt and AMPK OPEN">
    <entity charOffset="34-43" id="08741b63c08d63c1a86dd3ef3ce3d1bfd39e560b.c0.e0" ontology_id="GO_0006914" text="autophagy" type="gene_function"/>
    <entity charOffset="44-53" id="08741b63c08d63c1a86dd3ef3ce3d1bfd39e560b.c0.e1" ontology_id="GO_0023052" text="signaling" type="gene_function"/>
    <entity charOffset="138-142" id="08741b63c08d63c1a86dd3ef3ce3d1bfd39e560b.c0.e2" ontology_id="GO_0004691" text="AMPK" type="gene_function"/>
