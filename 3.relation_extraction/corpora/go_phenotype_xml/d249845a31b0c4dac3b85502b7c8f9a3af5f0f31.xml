    <entity charOffset="59-65" id="d249845a31b0c4dac3b85502b7c8f9a3af5f0f31.c0.e0" ontology_id="GO_0019012" text="virion" type="gene_function"/>
    <entity charOffset="66-74" id="d249845a31b0c4dac3b85502b7c8f9a3af5f0f31.c0.e1" ontology_id="GO_0031975" text="envelope" type="gene_function"/>
