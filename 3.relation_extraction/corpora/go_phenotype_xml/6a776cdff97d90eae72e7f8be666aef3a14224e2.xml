  <chunk id="6a776cdff97d90eae72e7f8be666aef3a14224e2.c0" text="Membrane Fusion and Cell Entry of XMRV Are pH- Independent and Modulated by the Envelope Glycoprotein's Cytoplasmic Tail">
    <entity charOffset="0-8" id="6a776cdff97d90eae72e7f8be666aef3a14224e2.c0.e0" ontology_id="GO_0016020" text="Membrane" type="gene_function"/>
