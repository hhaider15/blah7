  <chunk id="d117c8256785f2ff80a40fba878aa4f022d8310b.c0" text="Molecular Sciences SARS Coronavirus Papain-Like Protease Inhibits the TLR7 Signaling Pathway through Removing Lys63-Linked Polyubiquitination of TRAF3 and TRAF6">
    <entity charOffset="70-92" id="d117c8256785f2ff80a40fba878aa4f022d8310b.c0.e0" ontology_id="GO_0034154" text="TLR7 Signaling Pathway" type="gene_function"/>
