  <chunk id="3c988a00ed94308e36a8814b391ead01480efb60.c0" text="A Single Polar Residue and Distinct Membrane Topologies Impact the Function of the Infectious Bronchitis Coronavirus E Protein">
    <entity charOffset="36-44" id="3c988a00ed94308e36a8814b391ead01480efb60.c0.e0" ontology_id="GO_0016020" text="Membrane" type="gene_function"/>
    <entity charOffset="94-104" id="3c988a00ed94308e36a8814b391ead01480efb60.c0.e1" ontology_id="HP_0012387" text="Bronchitis" type="phenotype"/>
    <pair e1="3c988a00ed94308e36a8814b391ead01480efb60.c0.e0" e2="3c988a00ed94308e36a8814b391ead01480efb60.c0.e1" id="3c988a00ed94308e36a8814b391ead01480efb60.c0.p0" relation="true"/>
