  <chunk id="1dad67bd586092707fb9e7d311d465b82b85425f.c0" text="Infectious Bronchitis Virus Generates Spherules from Zippered Endoplasmic Reticulum Membranes">
    <entity charOffset="11-21" id="1dad67bd586092707fb9e7d311d465b82b85425f.c0.e0" ontology_id="HP_0012387" text="Bronchitis" type="phenotype"/>
