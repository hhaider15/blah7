  <chunk id="580ef02f408295bba21f0ad87b6eba6ba9676163.c0" text="Significant Prognostic Predictor for Patients with Severe Sepsis and Septic Shock">
    <entity charOffset="51-57" id="580ef02f408295bba21f0ad87b6eba6ba9676163.c0.e0" ontology_id="HP_0012828" text="Severe" type="phenotype"/>
