  <chunk id="d077e86d41dd4161565d2edf1446d8bc4ec5ed0a.c0" text="Genetic Variation in the TNF Gene Is Associated with Susceptibility to Severe Sepsis, but Not with Mortality">
    <entity charOffset="71-77" id="d077e86d41dd4161565d2edf1446d8bc4ec5ed0a.c0.e0" ontology_id="HP_0012828" text="Severe" type="phenotype"/>
