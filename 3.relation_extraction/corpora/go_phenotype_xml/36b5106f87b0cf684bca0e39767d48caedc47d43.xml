  <chunk id="36b5106f87b0cf684bca0e39767d48caedc47d43.c0" text="The Role of Mannose-Binding Lectin in Severe Sepsis and Septic Shock">
    <entity charOffset="38-44" id="36b5106f87b0cf684bca0e39767d48caedc47d43.c0.e0" ontology_id="HP_0012828" text="Severe" type="phenotype"/>
