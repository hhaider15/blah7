  <chunk id="ceab18d97f09ef0d9030f0c808f7ab5ab1c7d583.c0" text="NUDT2 Disruption Elevates Diadenosine Tetraphosphate (Ap 4 A) and Down-Regulates Immune Response and Cancer Promotion Genes">
    <entity charOffset="81-96" id="ceab18d97f09ef0d9030f0c808f7ab5ab1c7d583.c0.e0" ontology_id="GO_0006955" text="Immune Response" type="gene_function"/>
