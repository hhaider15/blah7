  <chunk id="4ea207323d2fedf5b4f5a4cbb8fd1acd100df8a8.c0" text="Mutagenesis of the fusion peptide-like domain of hepatitis C virus E1 glycoprotein: involvement in cell fusion and virus entry">
    <entity charOffset="49-58" id="4ea207323d2fedf5b4f5a4cbb8fd1acd100df8a8.c0.e0" ontology_id="HP_0012115" text="hepatitis" type="phenotype"/>
