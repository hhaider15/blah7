  <chunk id="c66b53fa3a31eb9fcc20c56830c89061b7caed49.c0" text="Increasing the translation of mouse models of MERS coronavirus pathogenesis through kinetic hematological analysis">
    <entity charOffset="15-26" id="c66b53fa3a31eb9fcc20c56830c89061b7caed49.c0.e0" ontology_id="GO_0006412" text="translation" type="gene_function"/>
    <entity charOffset="63-75" id="c66b53fa3a31eb9fcc20c56830c89061b7caed49.c0.e1" ontology_id="GO_0009405" text="pathogenesis" type="gene_function"/>
