  <chunk id="8bf449477aff84ef0591f10731a654610a5126b7.c0" text="Gene silencing of TACE enhances plaque stability and improves vascular remodeling in a rabbit model of atherosclerosis">
    <entity charOffset="0-14" id="8bf449477aff84ef0591f10731a654610a5126b7.c0.e0" ontology_id="GO_0016458" text="Gene silencing" type="gene_function"/>
