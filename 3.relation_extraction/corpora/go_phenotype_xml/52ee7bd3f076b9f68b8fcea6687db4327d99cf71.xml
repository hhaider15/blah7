  <chunk id="52ee7bd3f076b9f68b8fcea6687db4327d99cf71.c0" text="Mink (Neovison vison) kits with pre-weaning diarrhea have elevated serum amyloid A levels and intestinal pathomorphological similarities with New Neonatal Porcine Diarrhea Syndrome Open Access Acta Veterinaria Scandinavica">
    <entity charOffset="44-52" id="52ee7bd3f076b9f68b8fcea6687db4327d99cf71.c0.e0" ontology_id="HP_0002014" text="diarrhea" type="phenotype"/>
    <entity charOffset="163-171" id="52ee7bd3f076b9f68b8fcea6687db4327d99cf71.c0.e1" ontology_id="HP_0002014" text="Diarrhea" type="phenotype"/>
