  <chunk id="63b21e59e43206a09eaa36fdc5233f22af7515e3.c0" text="viruses Telomere Dynamics in Immune Senescence and Exhaustion Triggered by Chronic Viral Infection">
    <entity charOffset="75-82" id="63b21e59e43206a09eaa36fdc5233f22af7515e3.c0.e0" ontology_id="HP_0011010" text="Chronic" type="phenotype"/>
