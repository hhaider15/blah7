  <chunk id="1b1b133fc211d0bf8af604d58bedafd5fac7cde9.c0" text="Infectious Bronchitis Virus Nonstructural Protein 4 Alone Induces Membrane Pairing">
    <entity charOffset="11-21" id="1b1b133fc211d0bf8af604d58bedafd5fac7cde9.c0.e0" ontology_id="HP_0012387" text="Bronchitis" type="phenotype"/>
    <entity charOffset="66-74" id="1b1b133fc211d0bf8af604d58bedafd5fac7cde9.c0.e1" ontology_id="GO_0016020" text="Membrane" type="gene_function"/>
