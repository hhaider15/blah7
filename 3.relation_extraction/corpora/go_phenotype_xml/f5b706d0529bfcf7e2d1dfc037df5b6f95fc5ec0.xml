  <chunk id="f5b706d0529bfcf7e2d1dfc037df5b6f95fc5ec0.c0" text="Emergent severe acute respiratory distress syndrome caused by adenovirus type 55 in immunocompetent adults in 2013: a prospective observational study">
    <entity charOffset="9-15" id="f5b706d0529bfcf7e2d1dfc037df5b6f95fc5ec0.c0.e0" ontology_id="HP_0012828" text="severe" type="phenotype"/>
