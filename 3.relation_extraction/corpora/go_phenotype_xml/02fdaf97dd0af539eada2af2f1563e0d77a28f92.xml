  <chunk id="02fdaf97dd0af539eada2af2f1563e0d77a28f92.c0" text="Discovery of an antibody for pan- ebolavirus therapy OPEN">
    <entity charOffset="16-24" id="02fdaf97dd0af539eada2af2f1563e0d77a28f92.c0.e0" ontology_id="GO_0003823" text="antibody" type="gene_function"/>
