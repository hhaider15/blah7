  <chunk id="bec70be6631ac68e731b98fed55d2c28af45dc3d.c0" text="Fungal lectin MpL enables entry of protein drugs into cancer cells and their subcellular targeting">
    <entity charOffset="54-60" id="bec70be6631ac68e731b98fed55d2c28af45dc3d.c0.e0" ontology_id="HP_0002664" text="cancer" type="phenotype"/>
  </chunk>
  <chunk id="bec70be6631ac68e731b98fed55d2c28af45dc3d.c1" text="Lectins have been recognized as promising carrier molecules for targeted drug delivery. They specifically bind carbohydrate moieties on cell membranes and trigger cell internalization. Fungal lectin MpL (Macrolepiota procera lectin) does not provoke cancer cell cytotoxicity but is able to bind aminopeptidase N (CD13) and integrin α3β1, two glycoproteins that are overexpressed on the membrane of tumor cells.">
    <entity charOffset="42-49" id="bec70be6631ac68e731b98fed55d2c28af45dc3d.c1.e0" ontology_id="GO_0005215" text="carrier" type="gene_function"/>
    <entity charOffset="250-256" id="bec70be6631ac68e731b98fed55d2c28af45dc3d.c1.e1" ontology_id="HP_0002664" text="cancer" type="phenotype"/>
    <entity charOffset="386-394" id="bec70be6631ac68e731b98fed55d2c28af45dc3d.c1.e2" ontology_id="GO_0016020" text="membrane" type="gene_function"/>
    <entity charOffset="398-403" id="bec70be6631ac68e731b98fed55d2c28af45dc3d.c1.e3" ontology_id="HP_0002664" text="tumor" type="phenotype"/>
