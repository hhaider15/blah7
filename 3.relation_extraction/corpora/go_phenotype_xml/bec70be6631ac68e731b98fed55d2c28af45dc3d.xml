    <pair e1="bec70be6631ac68e731b98fed55d2c28af45dc3d.c1.e1" e2="bec70be6631ac68e731b98fed55d2c28af45dc3d.c1.e2" id="bec70be6631ac68e731b98fed55d2c28af45dc3d.c1.p2" relation="true"/>
    <pair e1="bec70be6631ac68e731b98fed55d2c28af45dc3d.c1.e2" e2="bec70be6631ac68e731b98fed55d2c28af45dc3d.c1.e3" id="bec70be6631ac68e731b98fed55d2c28af45dc3d.c1.p3" relation="true"/>
  </chunk>
  <chunk id="bec70be6631ac68e731b98fed55d2c28af45dc3d.c2" text="and clitocypin. The fused proteins followed the same endocytic route as the unlinked"/>
  <chunk id="bec70be6631ac68e731b98fed55d2c28af45dc3d.c3" text="to be a lectin that can enable protein drugs to enter cancer cells, enhance their internalization and sort them to lysosomes and the Golgi apparatus.">
