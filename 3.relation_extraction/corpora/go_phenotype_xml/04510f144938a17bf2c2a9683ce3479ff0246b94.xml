  <chunk id="04510f144938a17bf2c2a9683ce3479ff0246b94.c0" text="The protective role of microRNA-21 against coxsackievirus B3 infection through targeting the MAP2K3/P38 MAPK signaling pathway">
    <entity charOffset="104-108" id="04510f144938a17bf2c2a9683ce3479ff0246b94.c0.e0" ontology_id="GO_0004707" text="MAPK" type="gene_function"/>
