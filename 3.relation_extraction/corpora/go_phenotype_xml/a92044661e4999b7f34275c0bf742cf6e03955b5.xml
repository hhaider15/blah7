  <chunk id="a92044661e4999b7f34275c0bf742cf6e03955b5.c0" text="Deficient IFN Signaling by Myeloid Cells Leads to MAVS-Dependent Virus-Induced Sepsis">
    <entity charOffset="14-23" id="a92044661e4999b7f34275c0bf742cf6e03955b5.c0.e0" ontology_id="GO_0023052" text="Signaling" type="gene_function"/>
