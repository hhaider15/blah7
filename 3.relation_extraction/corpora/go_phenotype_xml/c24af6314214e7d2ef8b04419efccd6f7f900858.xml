  <chunk id="c24af6314214e7d2ef8b04419efccd6f7f900858.c0" text="Epithelial Cell Stretching and Luminal Acidification Lead to a Retarded Development of Stria Vascularis and Deafness in Mice Lacking Pendrin">
    <entity charOffset="39-52" id="c24af6314214e7d2ef8b04419efccd6f7f900858.c0.e0" ontology_id="GO_0045851" text="Acidification" type="gene_function"/>
