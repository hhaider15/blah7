  <chunk id="37e2ecefeffdf8d95f1509809b59fe0464abba44.c0" text="Prostaglandin E 2 stimulates normal bronchial epithelial cell growth through induction of c-Jun and PDK1, a kinase implicated in oncogenesis">
    <entity charOffset="57-68" id="37e2ecefeffdf8d95f1509809b59fe0464abba44.c0.e0" ontology_id="GO_0016049" text="cell growth" type="gene_function"/>
    <entity charOffset="62-68" id="37e2ecefeffdf8d95f1509809b59fe0464abba44.c0.e1" ontology_id="GO_0040007" text="growth" type="gene_function"/>
