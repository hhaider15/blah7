  <chunk id="bda64532f4a16e2885868385170f8bc89ab8f789.c0" text="Eukaryotic Initiation Factor 2α -a Downstream Effector of Mammalian Target of Rapamycin -Modulates DNA Repair and Cancer Response to Treatment">
    <entity charOffset="99-109" id="bda64532f4a16e2885868385170f8bc89ab8f789.c0.e0" ontology_id="GO_0006281" text="DNA Repair" type="gene_function"/>
    <entity charOffset="114-120" id="bda64532f4a16e2885868385170f8bc89ab8f789.c0.e1" ontology_id="HP_0002664" text="Cancer" type="phenotype"/>
    <pair e1="bda64532f4a16e2885868385170f8bc89ab8f789.c0.e0" e2="bda64532f4a16e2885868385170f8bc89ab8f789.c0.e1" id="bda64532f4a16e2885868385170f8bc89ab8f789.c0.p0" relation="true"/>
