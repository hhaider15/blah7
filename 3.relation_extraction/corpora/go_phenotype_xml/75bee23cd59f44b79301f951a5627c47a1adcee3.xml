  <chunk id="75bee23cd59f44b79301f951a5627c47a1adcee3.c0" text="Venezuelan Equine Encephalitis Virus Capsid Implicated in Infection-Induced Cell Cycle Delay in vitro">
    <entity charOffset="18-30" id="75bee23cd59f44b79301f951a5627c47a1adcee3.c0.e0" ontology_id="HP_0002383" text="Encephalitis" type="phenotype"/>
    <entity charOffset="76-86" id="75bee23cd59f44b79301f951a5627c47a1adcee3.c0.e1" ontology_id="GO_0007049" text="Cell Cycle" type="gene_function"/>
    <pair e1="75bee23cd59f44b79301f951a5627c47a1adcee3.c0.e0" e2="75bee23cd59f44b79301f951a5627c47a1adcee3.c0.e1" id="75bee23cd59f44b79301f951a5627c47a1adcee3.c0.p0" relation="true"/>
