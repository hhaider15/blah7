  <chunk id="d9367d8e090900c57b11a99848144bd83dcb80bb.c0" text="ApoD Mediates Binding of HDL to LDL and to Growing T24 Carcinoma">
    <entity charOffset="14-21" id="d9367d8e090900c57b11a99848144bd83dcb80bb.c0.e0" ontology_id="GO_0005488" text="Binding" type="gene_function"/>
