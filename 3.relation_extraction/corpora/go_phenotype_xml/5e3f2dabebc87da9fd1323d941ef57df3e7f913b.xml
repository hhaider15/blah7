  <chunk id="5e3f2dabebc87da9fd1323d941ef57df3e7f913b.c0" text="Targeting Hsp27/eIF4E interaction with phenazine compound: a promising alternative for castration-resistant prostate cancer treatment">
    <entity charOffset="108-123" id="5e3f2dabebc87da9fd1323d941ef57df3e7f913b.c0.e0" ontology_id="HP_0012125" text="prostate cancer" type="phenotype"/>
