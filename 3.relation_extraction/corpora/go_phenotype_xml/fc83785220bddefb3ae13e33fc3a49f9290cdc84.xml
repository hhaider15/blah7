  <chunk id="fc83785220bddefb3ae13e33fc3a49f9290cdc84.c0" text="Tonsillar cytokine expression between patients with tonsillar hypertrophy and recurrent tonsillitis">
    <entity charOffset="52-73" id="fc83785220bddefb3ae13e33fc3a49f9290cdc84.c0.e0" ontology_id="HP_0030812" text="tonsillar hypertrophy" type="phenotype"/>
