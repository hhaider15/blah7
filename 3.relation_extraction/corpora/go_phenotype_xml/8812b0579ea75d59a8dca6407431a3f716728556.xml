  <chunk id="8812b0579ea75d59a8dca6407431a3f716728556.c0" text="Quantitative Proteomic Analysis Reveals Unfolded-Protein Response Involved in Severe Fever with Thrombocytopenia Syndrome Virus Infection CELLULAR RESPONSE TO INFECTION crossm">
    <entity charOffset="78-84" id="8812b0579ea75d59a8dca6407431a3f716728556.c0.e0" ontology_id="HP_0012828" text="Severe" type="phenotype"/>
