  <chunk id="7d9f6bae2b7ced5834b4e92e7ef805d85fdea405.c0" text="Characterization of Rift Valley Fever Virus MP-12 Strain Encoding NSs of Punta Toro Virus or Sandfly Fever Sicilian Virus">
    <entity charOffset="25-37" id="7d9f6bae2b7ced5834b4e92e7ef805d85fdea405.c0.e0" ontology_id="HP_0032249" text="Valley Fever" type="phenotype"/>
    <entity charOffset="32-37" id="7d9f6bae2b7ced5834b4e92e7ef805d85fdea405.c0.e1" ontology_id="HP_0001945" text="Fever" type="phenotype"/>
    <entity charOffset="101-106" id="7d9f6bae2b7ced5834b4e92e7ef805d85fdea405.c0.e2" ontology_id="HP_0001945" text="Fever" type="phenotype"/>
