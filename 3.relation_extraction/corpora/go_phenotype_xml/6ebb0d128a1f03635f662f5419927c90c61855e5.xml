  <chunk id="6ebb0d128a1f03635f662f5419927c90c61855e5.c0" text="Activation of JNK1/2 and p38 MAPK signaling pathways promotes enterovirus 71 infection in immature dendritic cells">
    <entity charOffset="29-33" id="6ebb0d128a1f03635f662f5419927c90c61855e5.c0.e0" ontology_id="GO_0004707" text="MAPK" type="gene_function"/>
