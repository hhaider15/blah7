  <chunk id="5a1a67485f8d803e18aeb66383d616ceac659b47.c0" text="Global miRNA expression profiling of domestic cat livers following acute Toxoplasma gondii infection">
    <entity charOffset="46-49" id="5a1a67485f8d803e18aeb66383d616ceac659b47.c0.e0" ontology_id="GO_0004096" text="cat" type="gene_function"/>
