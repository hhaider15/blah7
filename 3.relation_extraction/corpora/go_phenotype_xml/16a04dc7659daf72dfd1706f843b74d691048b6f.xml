  <chunk id="16a04dc7659daf72dfd1706f843b74d691048b6f.c0" text="Porcine epidemic diarrhea virus E protein causes endoplasmic reticulum stress and up-regulates interleukin-8 expression">
    <entity charOffset="17-25" id="16a04dc7659daf72dfd1706f843b74d691048b6f.c0.e0" ontology_id="HP_0002014" text="diarrhea" type="phenotype"/>
