  <chunk id="b285ac5faca47fd0d835d385a193bb169447015f.c0" text="TGEV infection up-regulates FcRn expression via activation of NF-κB signaling OPEN">
    <entity charOffset="68-77" id="b285ac5faca47fd0d835d385a193bb169447015f.c0.e0" ontology_id="GO_0023052" text="signaling" type="gene_function"/>
