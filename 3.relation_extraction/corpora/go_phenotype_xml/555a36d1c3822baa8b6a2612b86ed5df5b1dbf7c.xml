  <chunk id="555a36d1c3822baa8b6a2612b86ed5df5b1dbf7c.c0" text="Enhanced inflammation in New Zealand white rabbits when MERS-CoV reinfection occurs in the absence of neutralizing antibody Author summary">
    <entity charOffset="115-123" id="555a36d1c3822baa8b6a2612b86ed5df5b1dbf7c.c0.e0" ontology_id="GO_0003823" text="antibody" type="gene_function"/>
    <entity charOffset="115-123" id="555a36d1c3822baa8b6a2612b86ed5df5b1dbf7c.c0.e1" ontology_id="GO_0042571" text="antibody" type="gene_function"/>
