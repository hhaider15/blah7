  <chunk id="f89c1c613071fcb7fdb5d956e91a807b83456ea3.c4" text="Conclusion: TNFα produces a nasal inflammatory response in humans that is characterised by late phase (i.e., 24 hours post challenge) neutrophil activity and plasma exudation.">
    <entity charOffset="34-55" id="f89c1c613071fcb7fdb5d956e91a807b83456ea3.c4.e0" ontology_id="GO_0006954" text="inflammatory response" type="gene_function"/>
