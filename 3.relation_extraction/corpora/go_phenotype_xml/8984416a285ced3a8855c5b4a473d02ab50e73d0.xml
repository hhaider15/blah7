  <chunk id="8984416a285ced3a8855c5b4a473d02ab50e73d0.c0" text="Three-Dimensional Architecture and Biogenesis of Membrane Structures Associated with Hepatitis C Virus Replication">
    <entity charOffset="49-57" id="8984416a285ced3a8855c5b4a473d02ab50e73d0.c0.e0" ontology_id="GO_0016020" text="Membrane" type="gene_function"/>
