  <chunk id="ddfd6e95ebc735146d0f0d6ea7faacaf99d9613c.c0" text="SARS-CoV fusion peptides induce membrane surface ordering and curvature OPEN">
    <entity charOffset="32-40" id="ddfd6e95ebc735146d0f0d6ea7faacaf99d9613c.c0.e0" ontology_id="GO_0016020" text="membrane" type="gene_function"/>
