    <entity charOffset="267-277" id="c5f1d681eb128c9c044a7f5ced6e0e943b201e93.c3.e2" ontology_id="GO_0007049" text="cell cycle" type="gene_function"/>
    <entity charOffset="267-284" id="c5f1d681eb128c9c044a7f5ced6e0e943b201e93.c3.e3" ontology_id="GO_0007050" text="cell cycle arrest" type="gene_function"/>
